odm:Address a sh:NodeShape ;
    sh:closed true ;
    sh:description "The postal address for a user, location, or organization." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:GeoPosition ;
            sh:description "GeoPosition reference: The geographical position using the World Geodetic System WGS84." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:geoPosition ],
        [ sh:class odm:HouseNumber ;
            sh:description "HouseNumber reference: The house number part of a user's postal address." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:houseNumber ],
        [ sh:class odm:City ;
            sh:description "City reference: The city name part of a user's postal address." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:city ],
        [ sh:class odm:Country ;
            sh:description "Country reference: The country name part of a user's postal address. For CDISC SDTM or trial registry applications, this must be represented by an ISO 3166 3-letter or US-GENC country code (e.g., FRA for France, JPN for Japan)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:country ],
        [ sh:class odm:PostalCode ;
            sh:description "PostalCode reference: The postal code part of a user's postal address." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:postalCode ],
        [ sh:class odm:StreetName ;
            sh:description "StreetName reference: The street name part of a user's postal address." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:streetName ],
        [ sh:class odm:StateProv ;
            sh:description "StateProv reference: The state or province name part of a user's postal address." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:stateProv ],
        [ sh:class odm:OtherText ;
            sh:description "OtherText reference: Any other text needed as part of a user's postal address." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:otherText ] ;
    sh:targetClass odm:Address .

odm:MethodSignature a sh:NodeShape ;
    sh:closed true ;
    sh:description "A MethodSignature defines the parameters and return values for a method. The MethodSignature improves traceability while enhancing the ability for automation engines to execute a MethodDef's FormalExpression. Most Methods use one or more input parameters and return one or more values." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:ReturnValue ;
            sh:description "ReturnValue reference: The ReturnValue element represents a return value used as part of a MethodSignature in MethodDef, ConditionDef, or RangeCheck. A return value identifies values passed from the Method to the calling element. A ReturnValue may be computed by a FormalExpression." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:returnValue ],
        [ sh:class odm:Parameter ;
            sh:description "Parameter reference: The Parameter element represents a method parameter used as part of a MethodSignature in MethodDef, ConditionDef, or RangeCheck." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:parameter ] ;
    sh:targetClass odm:MethodSignature .

odm:Telecom a sh:NodeShape ;
    sh:closed true ;
    sh:description "The telecommunication contacts points of a user, a location, or an organization. The Type attribute designates the type of contact." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:in ( "Email" "Pager" "Phone" "Fax" "SMS" "URL" "Other" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:telecomType ],
        [ sh:description "Human-readable designation of the trial phase." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:value ] ;
    sh:targetClass odm:Telecom .

odm:ItemGroupData a sh:NodeShape ;
    sh:closed true ;
    sh:description "Clinical data corresponding to an ItemGroupRef defined in the active MetaDataVersion." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:ItemData ;
            sh:description "ItemData reference: The ItemData element is used for transmission of the clinical data for an item. The model does not support repeating items within a single item group." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:itemData ],
        [ sh:description "A key used to distinguish between repeats of the same type of item group." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:itemGroupRepeatKey ],
        [ sh:class odm:Annotation ;
            sh:description "Annotation reference: A general note about clinical data. If an annotation has both a comment and flags, the flags should be related to the comment." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path odm:annotation ],
        [ sh:description "Reference to an ItemGroupDef for the MetaDataVersion identified in the ClinicalData element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:itemGroupOID ],
        [ sh:class odm:Signature ;
            sh:description "Signature reference: An electronic signature applies to a collection of clinical data. This indicates that some user accepts legal responsibility for that data. See 21 CFR Part 11. The signature identifies the person signing, the location of signing, the signature meaning (via the referenced SignatureDef), the date and time of signing, and (in the case of a digital signature) an encrypted hash of the included data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path odm:signature ],
        [ sh:class odm:ItemGroupData ;
            sh:description "ItemGroupData reference: Clinical data corresponding to an ItemGroupRef defined in the active MetaDataVersion." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:itemGroupData ],
        [ sh:description "Unique sequence # for each ItemGroupData child element (record) in the container element. The ItemGroupDataSeq attribute doesn’t have any other meaning than the sequence in which the items are saved and exchanged for each ItemGroupDef. It is equivalent to the observation # in a dataset." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:itemGroupDataSeq ],
        [ sh:description "The TransactionType attribute need not be present in a Snapshot document." ;
            sh:in ( "Insert" "Update" "Remove" "Upsert" "Context" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:transactionType ],
        [ sh:class odm:Query ;
            sh:description "Query reference: The Query element represents a request for clarification on a data item collected for a clinical trial, specifically a request from a sponsor or sponsor’s representative to an investigator to resolve an error or inconsistency discovered during data review. Queries can be created manually by individuals such as site monitors or data managers or automatically by systems. The full text of the Query exists in the Value child element. The optional Name attribute provide the means to provide a short identifier that can be included in listing or user interfaces." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path odm:query ],
        [ sh:class odm:AuditRecord ;
            sh:description "AuditRecord reference: An AuditRecord carries information pertaining to the creation, deletion, or modification of clinical data. This information includes who performed that action, and where, when, and why that action was performed.AuditRecord information describes a change to clinical data, but is not itself clinical data. The value of some clinical data can always be changed by a subsequent transaction, but history cannot be changed, only added to." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:auditRecord ] ;
    sh:targetClass odm:ItemGroupData .

odm:DocumentRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "Links to a leaf element with the location of the document." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:PDFPageRef ;
            sh:description "The PDFPageRef element is a container for page references in a PDF file." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:pDFPageRef ],
        [ sh:description "Reference to the unique ID of the Leaf element that contains the location of a file containing a document." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:leafID ] ;
    sh:targetClass odm:DocumentRef .

odm:FormalExpression a sh:NodeShape ;
    sh:closed true ;
    sh:description "A FormalExpression used within a ConditionDef or a RangeCheck must evaluate to True or False. A FormalExpression referenced within a MethodDef having Type Imputation, Computation, or Transpose must evaluate to the correct DataType for an Item that may be imputed or computed using the Method. A FormalExpression gets parameter and return value definitions from the MethodSignature element. The data types in the MethodSignature parameters and return values must match the corresponding data types in the FormalExpression." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Code ;
            sh:description "A string pattern that identifies a concept as defined by the code system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:code ],
        [ sh:class odm:ExternalCodeLib ;
            sh:description "ExternalCodeLib reference: The ExternalCodeLib element references a FormalExpression in an external code library, such as a file or GitHub. The intention is to make it possible to reference existing code libraries where the code is maintained as well as making it simpler to include longer, more complex FormalExpressions. The Library attribute provides the name of the external library, whereas ref or href provides a reference to the repository that can be used to retrieve the code. The Method attribute provides the name of the method in the file referenced for cases where multiple methods are provided in the source code file. The Version element provides the version of the external FormalExpression code referenced." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:externalCodeLib ],
        [ sh:description "A free-form qualifier to suggest an appropriate computer language to be used when evaluating the FormalExpression content." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:context ] ;
    sh:targetClass odm:FormalExpression .

odm:Query a sh:NodeShape ;
    sh:closed true ;
    sh:description "The Query element represents a request for clarification on a data item collected for a clinical trial, specifically a request from a sponsor or sponsor’s representative to an investigator to resolve an error or inconsistency discovered during data review. Queries can be created manually by individuals such as site monitors or data managers or automatically by systems. The full text of the Query exists in the Value child element. The optional Name attribute provide the means to provide a short identifier that can be included in listing or user interfaces." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Value ;
            sh:description "Human-readable designation of the trial phase." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:value ],
        [ sh:description "Origin of the Query." ;
            sh:in ( "System" "Data Management" "Site Monitor" "Coding System" "Safety Reviewer" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:source ],
        [ sh:description "When was this Query updated? Will correspond to the creation date or the last updated date?" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path odm:lastUpdateDatetime ],
        [ sh:description "Query unique identifier" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Element upon which the Query is raised. The parent element is the Target when the Target attribute is omitted." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:target ],
        [ sh:description "Status of the Query" ;
            sh:in ( "Candidate" "Open" "Answered" "Closed" "Cancelled" "Resolved" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path odm:state ],
        [ sh:description "Name for a query that can be used to identify the query in a listing or user interface." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:name ],
        [ sh:class odm:AuditRecord ;
            sh:description "AuditRecord reference: An AuditRecord carries information pertaining to the creation, deletion, or modification of clinical data. This information includes who performed that action, and where, when, and why that action was performed.AuditRecord information describes a change to clinical data, but is not itself clinical data. The value of some clinical data can always be changed by a subsequent transaction, but history cannot be changed, only added to." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:auditRecord ],
        [ sh:description "Indicates whether Is the Query was raised manually by a user or automatically via an edit check." ;
            sh:in ( "Manual" "System" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:type ] ;
    sh:targetClass odm:Query .

odm:Signature a sh:NodeShape ;
    sh:closed true ;
    sh:description "An electronic signature applies to a collection of clinical data. This indicates that some user accepts legal responsibility for that data. See 21 CFR Part 11. The signature identifies the person signing, the location of signing, the signature meaning (via the referenced SignatureDef), the date and time of signing, and (in the case of a digital signature) an encrypted hash of the included data." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:DateTimeStamp ;
            sh:description "DateTimeStamp reference: Date and time when an action was performed." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:dateTimeStamp ],
        [ sh:class odm:UserRef ;
            sh:description "UserRef reference: A reference to information about a specific user of a clinical data collection or data management system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:userRef ],
        [ sh:class odm:LocationRef ;
            sh:description "LocationRef reference: A reference to the user's physical location." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:locationRef ],
        [ sh:class odm:SignatureRef ;
            sh:description "SignatureRef reference: A reference to the signature meaning." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:signatureRef ],
        [ sh:description "Unique identifier for the leaf that is referenced." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:iD ] ;
    sh:targetClass odm:Signature .

odm:WorkflowRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "The WorkflowRef references a workflow definition" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a WorkflowDef" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:workflowOID ] ;
    sh:targetClass odm:WorkflowRef .

odm:Annotation a sh:NodeShape ;
    sh:closed true ;
    sh:description "A general note about clinical data. If an annotation has both a comment and flags, the flags should be related to the comment." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Flag ;
            sh:description "Flag reference: A machine-processable annotation." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:flag ],
        [ sh:description "Identifies the transaction type when /ODM/@FileType is Transactional and there is no child element." ;
            sh:in ( "Insert" "Update" "Remove" "Upsert" "Context" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:transactionType ],
        [ sh:class odm:Comment ;
            sh:description "Comment reference: A free-text (uninterpreted) comment about clinical data. The comment may have come from the sponsor or the clinical site." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:comment ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:coding ],
        [ sh:description "When more than 1 Value element exists this attribute uniquely identifies each Value and defines the order of a Value in a list of Values." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:seqNum ],
        [ sh:description "Unique identifier for the leaf that is referenced." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:iD ] ;
    sh:targetClass odm:Annotation .

odm:AuditRecord a sh:NodeShape ;
    sh:closed true ;
    sh:description "An AuditRecord carries information pertaining to the creation, deletion, or modification of clinical data. This information includes who performed that action, and where, when, and why that action was performed.AuditRecord information describes a change to clinical data, but is not itself clinical data. The value of some clinical data can always be changed by a subsequent transaction, but history cannot be changed, only added to." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:LocationRef ;
            sh:description "LocationRef reference: A reference to the user's physical location." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:locationRef ],
        [ sh:description "Indicates that the action was made by the system rather than a data entry form user action." ;
            sh:in ( "Yes" "No" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:usedMethod ],
        [ sh:class odm:DateTimeStamp ;
            sh:description "DateTimeStamp reference: Date and time when an action was performed." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:dateTimeStamp ],
        [ sh:class odm:ReasonForChange ;
            sh:description "ReasonForChange reference: A user-supplied reason for a data change." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:reasonForChange ],
        [ sh:description "Identifies the phase of data processing in which update action occurred." ;
            sh:in ( "Monitoring" "DataManagement" "DBAudit" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:editPoint ],
        [ sh:class odm:SourceID ;
            sh:description "SourceID reference: Information that identifies the source of the data within an originating system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:sourceID ],
        [ sh:class odm:UserRef ;
            sh:description "UserRef reference: A reference to information about a specific user of a clinical data collection or data management system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:userRef ] ;
    sh:targetClass odm:AuditRecord .

odm:Alias a sh:NodeShape ;
    sh:closed true ;
    sh:description "An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Alternate name of the parent element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Identifies applicable domain or scope of the mapping." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:context ] ;
    sh:targetClass odm:Alias .

odm:TranslatedText a sh:NodeShape ;
    sh:closed true ;
    sh:description "Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Code representing the language of the enclosed text value. Default value depends on locale. Note: The xml:lang attribute is part of the XML standard. See IETF Trust, Tags for Identifying Languages" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:language ],
        [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:content ],
        [ sh:description "Contains IANA media type of the TranslatedText element contents." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:type ] ;
    sh:targetClass odm:TranslatedText .

odm:Coding a sh:NodeShape ;
    sh:closed true ;
    sh:description "Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a local instance of the code system." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:ref ],
        [ sh:description "Identifies the version of the code system" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:systemVersion ],
        [ sh:description "A string pattern that identifies a concept as defined by the code system." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:code ],
        [ sh:description "Human readable name for the code system." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:systemName ],
        [ sh:description "Reference to a CommentDef that provides the rationale for the use of the Concept." ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path odm:commentOID ],
        [ sh:description "Used to link the value to a named MethodDef parameter." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:label ],
        [ sh:description "Identifies the code system that defines the code. If the code is taken from a code system resource then the URL for the code system should be used." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:system ],
        [ sh:description "URI reference to the Code definition." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:href ] ;
    sh:targetClass odm:Coding .

odm:Description a sh:NodeShape ;
    sh:closed true ;
    sh:description "A free-text description of the containing metadata component, unless restricted by Business Rules." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:TranslatedText ;
            sh:description "TranslatedText reference: Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:translatedText ] ;
    sh:targetClass odm:Description .

