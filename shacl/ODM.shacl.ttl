odm:WorkflowDef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A WorkflowDef defines an automated workflow for a study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:description ],
        [ sh:class odm:WorkflowEnd ;
            sh:description "WorkflowEnd reference: A WorkflowEnd references a structural element with which the workflows ends." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:workflowEnd ],
        [ sh:class odm:WorkflowStart ;
            sh:description "WorkflowStart reference: WorkflowStart references a structural element that begins the automated workflow." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:workflowStart ],
        [ sh:class odm:Branching ;
            sh:description "Branching reference: This element describes the branching in a workflow from a source (start) structural element to 2 or more target structural elements, over a Transition element." ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path odm:branching ],
        [ sh:description "Human readable label for the workflow." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
