    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:LegalReason ;
            sh:description "LegalReason reference: The responsibility statement associated with a signature (e.g., \"The signer accepts responsibility for the accuracy of this data.\")." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:legalReason ],
        [ sh:class odm:Meaning ;
            sh:description "Meaning reference: A short name or description for this signature. It should reflect the context of the signature and/or the text that appears when the signature is applied in the user interface." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:meaning ],
        [ sh:description "Unique identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Defines the type of electronic signature, including the meaning as required by 21 CFR Part 11 ." ;
            sh:in ( "Digital" "Electronic" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:methodology ] ;
