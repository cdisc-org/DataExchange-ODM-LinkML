@prefix odm: <http://www.cdisc.org/ns/odm/v2.0/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

odm:ODM a sh:NodeShape ;
    sh:closed true ;
    sh:description "Root element for ODM Documents. The ODM element is the top-level (root) element of each ODM document." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Indicates the intended usage of the ODM document. Archive - indicates that the file is intended to meet the requirements of an electronic record as defined in 21 CFR 11. Submission - indicates that the file is intended to be used for regulatory submission. Exchange - indicates that the file was generated to be imported into an ODM compliant system." ;
            sh:in ( "Archive" "Exchange" "Submission" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:context ],
        [ sh:description "A unique identifier for this file." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path odm:fileOID ],
        [ sh:description "Time of creation of the file containing the document." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path odm:creationDateTime ],
        [ sh:class odm:ReferenceData ;
            sh:description "ReferenceData reference: Reference data provides information on how to interpret clinical data. For example, reference data might include lab normal ranges. For a study that uses CDISC standards, reference data might include SDTM Trial Design datasets." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 14 ;
            sh:path odm:referenceData ],
        [ sh:class odm:Association ;
            sh:description "Association reference: An association permits an annotation to be placed on an ordered pair of entities rather than on just one. The first and second KeySets identify the start and end of the annotated \"link." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 16 ;
            sh:path odm:association ],
        [ sh:description "The version of the \"SourceSystem\" above." ;
            sh:maxCount 1 ;
            sh:order 10 ;
            sh:path odm:sourceSystemVersion ],
        [ sh:description "The organization that generated the ODM file." ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path odm:originator ],
        [ sh:description "The computer system or database management system that is the source of the information in this file." ;
            sh:maxCount 1 ;
            sh:order 9 ;
            sh:path odm:sourceSystem ],
        [ sh:description "The date/time at which the source database was queried in order to create this document." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:asOfDateTime ],
        [ sh:description "The version of the ODM standard used." ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path odm:oDMVersion ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 11 ;
            sh:path odm:description ],
        [ sh:description "Granularity is intended to give the sender a shorthand way to Describes the scope of information in the document, for certain common types of documents. All means the entire study; Metadata means the Study/MetaDataVersion element; AdminData and ReferenceData mean the corresponding elements; AllClinicalData means all of the ClinicalData collected for the study. SingleSite, means all of the Clinical Data for a single study site. SingleSubject means all of the Clinical Data for a single Subject." ;
            sh:in ( "All" "Metadata" "AdminData" "ReferenceData" "AllClinicalData" "SingleSite" "SingleSubject" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:granularity ],
        [ sh:description "Reference to the previous file (if any) in a series." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:priorFileOID ],
        [ sh:class odm:AdminData ;
            sh:description "AdminData reference: Administrative information about users, locations, organizations, and electronic signatures." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 13 ;
            sh:path odm:adminData ],
        [ sh:class odm:Study ;
            sh:description "Study reference: This element collects static structural information about an individual study." ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path odm:study ],
        [ sh:class odm:ClinicalData ;
            sh:description "ClinicalData reference: Clinical data for 1 or more subjects." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 15 ;
            sh:path odm:clinicalData ],
        [ sh:description "Snapshot means that the document contains only the current state of the data and metadata it describes, and no transactional history. Transactional means that the document may contain more than one instance per data point. Query means the document contains only ClinicalData/Query elements." ;
            sh:in ( "Snapshot" "Transactional" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:fileType ] ;
    sh:targetClass odm:ODM .

odm:AbsoluteTimingConstraint a sh:NodeShape ;
    sh:closed true ;
    sh:description "The element AbsoluteTimingConstraint is used to specify when an activity, represented by either a StudyEventGroup or StudyEvent, can take place." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Specifies the amount of time before the TimepointTarget that the activity may be started." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:timepointPreWindow ],
        [ sh:description "Reference to StudyEvent definition element." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:studyEventOID ],
        [ sh:description "Reference to StudyEventGroup definition element." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:studyEventGroupOID ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:description ],
        [ sh:description "Human readable name." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Specifies the targetted date, time, or datetime." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path odm:timepointTarget ],
        [ sh:description "Specifies the amount of time after the TimepointTarget that the activity can be started." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:timepointPostWindow ],
        [ sh:description "Unique identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ] ;
    sh:targetClass odm:AbsoluteTimingConstraint .

odm:AdminData a sh:NodeShape ;
    sh:closed true ;
    sh:description "Administrative information about users, locations, organizations, and electronic signatures." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a Study ." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:studyOID ],
        [ sh:class odm:Location ;
            sh:description "Location reference: A physical location associated with data collection and/or treatment of subjects." ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path odm:location ],
        [ sh:class odm:SignatureDef ;
            sh:description "SignatureDef reference: Provides Metadata for signatures included in the /ODM/ClinicalData." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path odm:signatureDef ],
        [ sh:class odm:Organization ;
            sh:description "Organization reference: An organization can reference a parent organization. Users may be associated with an Organization. An Organization may be associated with a Location. A User, Location, or Organization may have an address." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path odm:organization ],
        [ sh:class odm:User ;
            sh:description "User reference: Information about a specific user of a clinical data collection or data management system." ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path odm:user ] ;
    sh:targetClass odm:AdminData .

odm:AnnotatedCRF a sh:NodeShape ;
    sh:closed true ;
    sh:description "An Annotated Case Report Form (CRF) is a Portable File Format (PDF) document that provides the mapping of data collection fields to the variables or discrete variable values contained within the datasets." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:DocumentRef ;
            sh:description "Links to a def:leaf element with the location of the document." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:documentRef ] ;
    sh:targetClass odm:AnnotatedCRF .

odm:Arm a sh:NodeShape ;
    sh:closed true ;
    sh:description "An Arm element provides the declaration of a study arm. Arms do not have any ordering relative to one another." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Human readable name of the study arm." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:WorkflowRef ;
            sh:description "WorkflowRef reference: The WorkflowRef references a workflow definition" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:workflowRef ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:description ],
        [ sh:description "Unique identifier for the Arm definition." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ] ;
    sh:targetClass odm:Arm .

odm:Association a sh:NodeShape ;
    sh:closed true ;
    sh:description "An association permits an annotation to be placed on an ordered pair of entities rather than on just one. The first and second KeySets identify the start and end of the annotated \"link." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a MetaDataVersion element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:metaDataVersionOID ],
        [ sh:class odm:KeySet ;
            sh:description "KeySet reference: A KeySet references a single entity (e.g., a study, a subject, a study event). Only those attributes needed to specify the particular entity are required, and all others must be omitted (see Section 2.7, Clinical Data Keys)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:keySet ],
        [ sh:class odm:Annotation ;
            sh:description "Annotation reference: A general note about clinical data. If an annotation has both a comment and flags, the flags should be related to the comment." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path odm:annotation ],
        [ sh:description "Reference to an ODM Study element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyOID ] ;
    sh:targetClass odm:Association .

odm:Branching a sh:NodeShape ;
    sh:closed true ;
    sh:description "This element describes the branching in a workflow from a source (start) structural element to 2 or more target structural elements, over a Transition element." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Type of page for page references indicated in the PageRefs attribute." ;
            sh:in ( "Exclusive" "Parallel" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:type ],
        [ sh:description "Unique identifier of the version within the XML document." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "General observation Sub Class." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:DefaultTransition ;
            sh:description "DefaultTransition reference: The DefaultTransition references the Transition that needs to be executed when none of the TargetTransitions can be executed." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:defaultTransition ],
        [ sh:class odm:TargetTransition ;
            sh:description "TargetTransition reference: TargetTransition provides a reference to a Transition element that is the target of a branching." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:targetTransition ] ;
    sh:targetClass odm:Branching .

odm:CDISCNotes a sh:NodeShape ;
    sh:closed true ;
    sh:description "Explanatory text for the variable." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:TranslatedText ;
            sh:description "TranslatedText reference: Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:translatedText ] ;
    sh:targetClass odm:CDISCNotes .

odm:CRFCompletionInstructions a sh:NodeShape ;
    sh:closed true ;
    sh:description "Instructions for the clinical site on how to enter collected information on the CRF." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:TranslatedText ;
            sh:description "TranslatedText reference: Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:translatedText ] ;
    sh:targetClass odm:CRFCompletionInstructions .

odm:CheckValue a sh:NodeShape ;
    sh:closed true ;
    sh:description "A comparison value used in a range check." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:CheckValue .

odm:City a sh:NodeShape ;
    sh:closed true ;
    sh:description "The city name part of a user's postal address." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:City .

odm:Class a sh:NodeShape ;
    sh:closed true ;
    sh:description "The Class element identifies which predefined Class within the model applies to the definition of the dataset." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:SubClass ;
            sh:description "SubClass reference: This element contains SubClass definitions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:subClass ],
        [ sh:description "Name of the Class" ;
            sh:in ( <nci:ExtCodeID:C103375> <nci:ExtCodeID:C103371> <nci:ExtCodeID:C177921> <nci:ExtCodeID:C103372> <nci:ExtCodeID:C103373> <nci:ExtCodeID:C135396> <nci:ExtCodeID:C103374> <nci:ExtCodeID:C177922> <nci:ExtCodeID:C177923> <nci:ExtCodeID:C123454> <nci:ExtCodeID:C103376> <nci:ExtCodeID:C103377> <nci:ExtCodeID:C147271> <nci:ExtCodeID:C103378> <nci:ExtCodeID:C103379> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:name ] ;
    sh:targetClass odm:Class .

odm:ClinicalData a sh:NodeShape ;
    sh:closed true ;
    sh:description "Clinical data for 1 or more subjects." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:AuditRecord ;
            sh:description "AuditRecord reference: An AuditRecord carries information pertaining to the creation, deletion, or modification of clinical data. This information includes who performed that action, and where, when, and why that action was performed.AuditRecord information describes a change to clinical data, but is not itself clinical data. The value of some clinical data can always be changed by a subsequent transaction, but history cannot be changed, only added to." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:auditRecord ],
        [ sh:description "References the MetaDataVersion (within the above Study) that governs the data nested within this element. The StudyOID and MetaDataVersionOID attributes select a particular metadata version. All metadata references (OIDs) occurring within this ClinicalData element refer to definitions within the selected metadata version." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:metaDataVersionOID ],
        [ sh:class odm:Annotation ;
            sh:description "Annotation reference: A general note about clinical data. If an annotation has both a comment and flags, the flags should be related to the comment." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path odm:annotation ],
        [ sh:class odm:Query ;
            sh:description "Query reference: The Query element represents a request for clarification on a data item collected for a clinical trial, specifically a request from a sponsor or sponsor’s representative to an investigator to resolve an error or inconsistency discovered during data review. Queries can be created manually by individuals such as site monitors or data managers or automatically by systems. The full text of the Query exists in the Value child element. The optional Name attribute provide the means to provide a short identifier that can be included in listing or user interfaces." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path odm:query ],
        [ sh:description "References the Study that uses the data nested within this element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyOID ],
        [ sh:class odm:SubjectData ;
            sh:description "SubjectData reference: Clinical data for a single subject." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:subjectData ],
        [ sh:class odm:ItemGroupData ;
            sh:description "ItemGroupData reference: Clinical data corresponding to an ItemGroupRef defined in the active MetaDataVersion." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:itemGroupData ],
        [ sh:class odm:Signature ;
            sh:description "Signature reference: An electronic signature applies to a collection of clinical data. This indicates that some user accepts legal responsibility for that data. See 21 CFR Part 11. The signature identifies the person signing, the location of signing, the signature meaning (via the referenced SignatureDef), the date and time of signing, and (in the case of a digital signature) an encrypted hash of the included data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path odm:signature ] ;
    sh:targetClass odm:ClinicalData .

odm:Code a sh:NodeShape ;
    sh:closed true ;
    sh:description "Contains the source code that represents a FormalExpression in a given Context. The source code must be executable, and the MethodSignature defines the input parameters and return values for the code." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:Code .

odm:CodeList a sh:NodeShape ;
    sh:closed true ;
    sh:description "Defines a discrete set of permitted values for an item, or provides a reference to a codelist or dictionary maintained by an external organization via the Coding element, or a combination of both. Examples provided under Coding." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Human readable name for the Codelist." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Specifies the DataType for codes defined in this codelist." ;
            sh:in ( "integer" "decimal" "text" "string" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:dataType ],
        [ sh:description "Unique identifier for the Codelist element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:CodeListItem ;
            sh:description "CodeListItem reference: Defines an individual member value of a codelist. It may include a display value in the child Decode element" ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:codeListItem ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:coding ],
        [ sh:description "Reference to a Standard element." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:standardOID ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:description ],
        [ sh:description "Used when the controlled terminology includes a set of EnumeratedItem or CodeListItem elements as defined by the sponsor." ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:isNonStandard ],
        [ sh:class odm:Alias ;
            sh:description "Alias reference: An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 9 ;
            sh:path odm:alias ],
        [ sh:description "Reference to a CommentDef Element." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:commentOID ] ;
    sh:targetClass odm:CodeList .

odm:CodeListItem a sh:NodeShape ;
    sh:closed true ;
    sh:description "Defines an individual member value of a codelist. It may include a display value in the child Decode element" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Ordering on the CodeListItems (within a containing CodeListItem) for use whenever a list of Items is presented to a user. The ordering does not imply anything about event scheduling, time ordering, or data correctness." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:orderNumber ],
        [ sh:class odm:Alias ;
            sh:description "Alias reference: An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 9 ;
            sh:path odm:alias ],
        [ sh:description "Value of the codelist item (as it would occur in clinical data)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:codedValue ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:description ],
        [ sh:description "Reference to a CommentDef ." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:commentOID ],
        [ sh:description "Flag to indicate that the term represents \"other\" content." ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:other ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:coding ],
        [ sh:class odm:Decode ;
            sh:description "Decode reference: The displayed value relating to the CodeListItem/@CodedValue. This is often a label corresponding to a short name or alpha-numeric code. The actual Decode text is provided in a TranslatedText element so that it can be provided in different languages on a case report form or tabular data summary." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:decode ],
        [ sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:extendedValue ],
        [ sh:description "Numeric significance of the CodeListItem relative to others in the CodeList. The Rank attribute may be used where the relative value corresponding to an enumeration cannot or should not be determined by its lexical order. For example, if you have a list of enumerated text values including \"Low\", \"Medium\", and \"High\" and wish to assign these relative numeric values 1, 2, and 3 respectively, you should include a Rank attribute for each CodeListItem defined. Without the applied rank attribute, the normal lexical ordering would be \"High\", \"Low\", and \"Medium\"." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:rank ] ;
    sh:targetClass odm:CodeListItem .

odm:CodeListRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reference to a CodeList definition." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to the CodeList definition that provides the allowable values for ItemData that references the ItemDef." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:codeListOID ] ;
    sh:targetClass odm:CodeListRef .

odm:Comment a sh:NodeShape ;
    sh:closed true ;
    sh:description "A free-text (uninterpreted) comment about clinical data. The comment may have come from the sponsor or the clinical site." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:TranslatedText ;
            sh:description "TranslatedText reference: Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:translatedText ],
        [ sh:description "Source of the comment." ;
            sh:in ( "Sponsor" "Site" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:sponsorOrSite ] ;
    sh:targetClass odm:Comment .

odm:CommentDef a sh:NodeShape ;
    sh:closed true ;
    sh:description "The Comment element allows referencing short comments self-contained in the XML document or long comments normally included in external documents. For comments included in external documents, the reference could include specific pages of a document where the comments are included." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Description ;
            sh:description "Text of the comment." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:description ],
        [ sh:class odm:DocumentRef ;
            sh:description "The DocumentRef element is a container for page references in a PDF file." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:documentRef ],
        [ sh:description "Unique identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ] ;
    sh:targetClass odm:CommentDef .

odm:ConditionDef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A ConditionDef defines a boolean condition." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:description ],
        [ sh:description "Unique identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:Alias ;
            sh:description "Alias reference: An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:alias ],
        [ sh:description "Human-readable name for the Condition." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:FormalExpression ;
            sh:description "FormalExpression reference: A FormalExpression used within a ConditionDef or a RangeCheck must evaluate to True or False. A FormalExpression referenced within a MethodDef having Type Imputation, Computation, or Transpose must evaluate to the correct DataType for an Item that may be imputed or computed using the Method. A FormalExpression gets parameter and return value definitions from the MethodSignature element. The data types in the MethodSignature parameters and return values must match the corresponding data types in the FormalExpression." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:formalExpression ],
        [ sh:class odm:MethodSignature ;
            sh:description "MethodSignature reference: A MethodSignature defines the parameters and return values for a method. The MethodSignature improves traceability while enhancing the ability for automation engines to execute a MethodDef's FormalExpression. Most Methods use one or more input parameters and return one or more values." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:methodSignature ],
        [ sh:description "Reference to a CommentDef element." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:commentOID ] ;
    sh:targetClass odm:ConditionDef .

odm:Country a sh:NodeShape ;
    sh:closed true ;
    sh:description "The country name part of a user's postal address. For CDISC SDTM or trial registry applications, this must be represented by an ISO 3166 3-letter or US-GENC country code (e.g., FRA for France, JPN for Japan)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:Country .

odm:Decode a sh:NodeShape ;
    sh:closed true ;
    sh:description "The displayed value relating to the CodeListItem/@CodedValue. This is often a label corresponding to a short name or alpha-numeric code. The actual Decode text is provided in a TranslatedText element so that it can be provided in different languages on a case report form or tabular data summary." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:TranslatedText ;
            sh:description "TranslatedText reference: Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:translatedText ] ;
    sh:targetClass odm:Decode .

odm:DefaultTransition a sh:NodeShape ;
    sh:closed true ;
    sh:description "The DefaultTransition references the Transition that needs to be executed when none of the TargetTransitions can be executed." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to the Transition that is the default target of the branching." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:targetTransitionOID ] ;
    sh:targetClass odm:DefaultTransition .

odm:Definition a sh:NodeShape ;
    sh:closed true ;
    sh:description "Definition of the item." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:TranslatedText ;
            sh:description "TranslatedText reference: Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:translatedText ] ;
    sh:targetClass odm:Definition .

odm:DurationTimingConstraint a sh:NodeShape ;
    sh:closed true ;
    sh:description "The DurationTimingConstraint constrains the duration of an activity represented by a study, epoch, StudyEventGroupDef, StudyEventDef, ItemGroupDef, or ItemDef. It is used to constrain the duration of the visit, activity, or any other structural element." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "OID of a structural element such as a Study, Epoch, StudyEventGroup, StudyEvent, ItemGroup, Item" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:structuralElementOID ],
        [ sh:description "Constrains the duration of an activity represented by a Study, Epoch, StudyEventGroupDef, StudyEventDef, ItemGroupDef or ItemDef. Specifies the planned duration of the referenced structural element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path odm:durationTarget ],
        [ sh:description "Defines the amount of time by which the targetted duration may be increased." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:durationPostWindow ],
        [ sh:description "Human readable name for the DurationTimingConstraint." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Unique identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:description ],
        [ sh:description "Defines the amount of time by which the targetted duration may be reduced." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:durationPreWindow ] ;
    sh:targetClass odm:DurationTimingConstraint .

odm:Epoch a sh:NodeShape ;
    sh:closed true ;
    sh:description "The planned period of subjects' participation in the trial is divided into sequential epochs. Each epoch is a period of time that serves a purpose in the trial as a whole. Epochs cannot overlap. The sequence of the epoch in the study is provided by the SequenceNumber attribute, the first epoch in the study being assigned the sequence number 1. Sequence numbers are subsequent." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Human readable name of the study epoch." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:description ],
        [ sh:description "Unique identifier for the Epoch." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Order of the Epoch" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:sequenceNumber ] ;
    sh:targetClass odm:Epoch .

odm:ErrorMessage a sh:NodeShape ;
    sh:closed true ;
    sh:description "Error message provided to user when the range check fails." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:TranslatedText ;
            sh:description "TranslatedText reference: Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:translatedText ] ;
    sh:targetClass odm:ErrorMessage .

odm:ExclusionCriteria a sh:NodeShape ;
    sh:closed true ;
    sh:description "The ExclusionCriteria is a container element for Criterion elements describing exclusion criteria for subjects in the study. When a list is provided, not meeting any of the criteria in the list may lead to exclusion of enrollment in the study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Criterion ;
            sh:description "Criterion reference: The Criterion represents either an inclusion or an exclusion criterion, depending on the parent element (i.e., InclusionCriteria, ExclusionCriteria)." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:criterion ] ;
    sh:targetClass odm:ExclusionCriteria .

odm:ExternalCodeLib a sh:NodeShape ;
    sh:closed true ;
    sh:description "The ExternalCodeLib element references a FormalExpression in an external code library, such as a file or GitHub. The intention is to make it possible to reference existing code libraries where the code is maintained as well as making it simpler to include longer, more complex FormalExpressions. The Library attribute provides the name of the external library, whereas ref or href provides a reference to the repository that can be used to retrieve the code. The Method attribute provides the name of the method in the file referenced for cases where multiple methods are provided in the source code file. The Version element provides the version of the external FormalExpression code referenced." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The version of the code contained in the external library." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:version ],
        [ sh:description "Reference to a local instance (e.g. file) of the external library containing the FormalExpression code." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:ref ],
        [ sh:description "URL of an external code external library such as GitHub or Bitbucket." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:href ],
        [ sh:description "The name of the method or function that contains the FormalExpression code." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:method ],
        [ sh:description "The name of the external library containing the FormalExpression." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:library ] ;
    sh:targetClass odm:ExternalCodeLib .

odm:FamilyName a sh:NodeShape ;
    sh:closed true ;
    sh:description "The user's surname (family name)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:FamilyName .

odm:Flag a sh:NodeShape ;
    sh:closed true ;
    sh:description "A machine-processable annotation." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:FlagValue ;
            sh:description "FlagValue reference: The value of the flag. The meaning of this value is typically dependent on the associated FlagType. The actual value must be a member of the referenced CodeList" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:flagValue ],
        [ sh:class odm:FlagType ;
            sh:description "FlagType reference: The type of flag. This determines the purpose and semantics of the flag." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:flagType ] ;
    sh:targetClass odm:Flag .

odm:FlagType a sh:NodeShape ;
    sh:closed true ;
    sh:description "The type of flag. This determines the purpose and semantics of the flag." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to the CodeList definition." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:codeListOID ],
        [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:content ] ;
    sh:targetClass odm:FlagType .

odm:FlagValue a sh:NodeShape ;
    sh:closed true ;
    sh:description "The value of the flag. The meaning of this value is typically dependent on the associated FlagType. The actual value must be a member of the referenced CodeList" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to the CodeList definition." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:codeListOID ],
        [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:content ] ;
    sh:targetClass odm:FlagValue .

odm:FullName a sh:NodeShape ;
    sh:closed true ;
    sh:description "The user's full formal name. May be a combination of Prefix, GivenName, FamilyName & Suffix. Intended to be used for display." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:FullName .

odm:GeoPosition a sh:NodeShape ;
    sh:closed true ;
    sh:description "The geographical position using the World Geodetic System WGS84." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Latitude component of geoposition coordinate in decimal degrees degrees. May require conversion from degrees, minutes, seconds format." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:latitude ],
        [ sh:description "Height above sea level in meters." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:altitude ],
        [ sh:description "Longitude component of geoposition coordinates in decimal degrees. May require conversion from degrees, minutes, seconds format." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:longitude ] ;
    sh:targetClass odm:GeoPosition .

odm:GivenName a sh:NodeShape ;
    sh:closed true ;
    sh:description "The user's initial given name or all given names." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:GivenName .

odm:HouseNumber a sh:NodeShape ;
    sh:closed true ;
    sh:description "The house number part of a user's postal address." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:HouseNumber .

odm:Image a sh:NodeShape ;
    sh:closed true ;
    sh:description "A visual depiction of the user." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Media type of the image" ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:mimeType ],
        [ sh:description "URL of the location of the picture" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:href ],
        [ sh:description "The file name of or file path to the picture" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:imageFileName ] ;
    sh:targetClass odm:Image .

odm:ImplementationNotes a sh:NodeShape ;
    sh:closed true ;
    sh:description "Further information, such as rationale and implementation instructions, on how to implement the CRF data collection fields." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:TranslatedText ;
            sh:description "TranslatedText reference: Human-readable text that is appropriate for a particular language. TranslatedText elements typically occur in a series, presenting a set of alternative textual renditions for different languages and types." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:translatedText ] ;
    sh:targetClass odm:ImplementationNotes .

odm:Include a sh:NodeShape ;
    sh:closed true ;
    sh:description "The Include metadata element allows a reference to a prior metadata version." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to the location where the to be included Study-Metadata definition can be accessed. The href attribute allows to provide the location (as a URL) of the ODM where the to-be-included elements can be retrieved, in the case that the combination of the referenced study and metadataversion is not present in the same file. The reference can be to a file (e.g., \"file:///d:/MyStudies/MyStudy/PriorVersionODM.xml\") or be an API call (e.g., \" https://www.MyCompany.com/MyStudies?StudyOID=MyStudy&MetaDataVersionOID=MV.001 \")." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:href ],
        [ sh:description "References the Study that provides a prior metadata version. This attribute allows an Include element to reference a metadata version in another study. Thus, it is possible for many studies to share a set of common metadata definitions" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyOID ],
        [ sh:description "References a prior MetaDataVersion within the Study referenced by the StudyOID attribute." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:metaDataVersionOID ] ;
    sh:targetClass odm:Include .

odm:InclusionCriteria a sh:NodeShape ;
    sh:closed true ;
    sh:description "The InclusionCriteria is a container element for Criterion elements describing inclusion criteria for subjects in the study. When a list is provided, subjects must meet each of the criteria in the list in order to enroll in the study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Criterion ;
            sh:description "Criterion reference: The Criterion represents either an inclusion or an exclusion criterion, depending on the parent element (i.e., InclusionCriteria, ExclusionCriteria)." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:criterion ] ;
    sh:targetClass odm:InclusionCriteria .

odm:InclusionExclusionCriteria a sh:NodeShape ;
    sh:closed true ;
    sh:description "The InclusionExclusionCriteria element can contain 2 lists of Criterion elements, represented by the 2 elements InclusionCriteria and ExclusionCriteria. Together, these criteria determine the eligibility of a subject for the study. The actual condition to be evaluated is contained in an ODM ConditionDef, which is referenced by each Criterion‟s ConditionOID attribute." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:InclusionCriteria ;
            sh:description "InclusionCriteria reference: The InclusionCriteria is a container element for Criterion elements describing inclusion criteria for subjects in the study. When a list is provided, subjects must meet each of the criteria in the list in order to enroll in the study." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:inclusionCriteria ],
        [ sh:class odm:ExclusionCriteria ;
            sh:description "ExclusionCriteria reference: The ExclusionCriteria is a container element for Criterion elements describing exclusion criteria for subjects in the study. When a list is provided, not meeting any of the criteria in the list may lead to exclusion of enrollment in the study." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:exclusionCriteria ] ;
    sh:targetClass odm:InclusionExclusionCriteria .

odm:IntercurrentEvent a sh:NodeShape ;
    sh:closed true ;
    sh:description "The IntercurrentEvent element describes an intercurrent event for an estimand (e.g., treatment discontinuation)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:description ] ;
    sh:targetClass odm:IntercurrentEvent .

odm:InvestigatorRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "Provides a reference to the user who created the SubjectData record in the source system." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a User definition." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:userOID ] ;
    sh:targetClass odm:InvestigatorRef .

odm:ItemData a sh:NodeShape ;
    sh:closed true ;
    sh:description "The ItemData element is used for transmission of the clinical data for an item. The model does not support repeating items within a single item group." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Flag specifying that an item's value is to be set to null. In the interest of creating non-verbose XML instances, one should not use ItemData elements with IsNull set to \"Yes\" to indicate uncollected data. The better practice is to transmit only collected data. For use cases where data traceability is important, providing ItemData elements with IsNull=\"Yes\" maybe be useful. It is not necessary to provide an ItemData element with IsNull set to \"Yes\" in cases where the source system would not create a record." ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:isNull ],
        [ sh:class odm:Signature ;
            sh:description "Signature reference: An electronic signature applies to a collection of clinical data. This indicates that some user accepts legal responsibility for that data. See 21 CFR Part 11. The signature identifies the person signing, the location of signing, the signature meaning (via the referenced SignatureDef), the date and time of signing, and (in the case of a digital signature) an encrypted hash of the included data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path odm:signature ],
        [ sh:class odm:Query ;
            sh:description "Query reference: The Query element represents a request for clarification on a data item collected for a clinical trial, specifically a request from a sponsor or sponsor’s representative to an investigator to resolve an error or inconsistency discovered during data review. Queries can be created manually by individuals such as site monitors or data managers or automatically by systems. The full text of the Query exists in the Value child element. The optional Name attribute provide the means to provide a short identifier that can be included in listing or user interfaces." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path odm:query ],
        [ sh:class odm:AuditRecord ;
            sh:description "AuditRecord reference: An AuditRecord carries information pertaining to the creation, deletion, or modification of clinical data. This information includes who performed that action, and where, when, and why that action was performed.AuditRecord information describes a change to clinical data, but is not itself clinical data. The value of some clinical data can always be changed by a subsequent transaction, but history cannot be changed, only added to." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:auditRecord ],
        [ sh:description "Records the TransactionType for this ItemData instance in the source system." ;
            sh:in ( "Insert" "Update" "Remove" "Upsert" "Context" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:transactionType ],
        [ sh:class odm:Annotation ;
            sh:description "Annotation reference: A general note about clinical data. If an annotation has both a comment and flags, the flags should be related to the comment." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path odm:annotation ],
        [ sh:description "Reference to an ItemDef in the MetaDataVersion identified in the ClinicalData element. The referenced ItemDef defines the DataType of this item. The ItemOID attribute is used to identify a particular item definition. This value uniquely identifies an Item within the containing ItemGroup." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:itemOID ],
        [ sh:class odm:Value ;
            sh:description "Human-readable designation of the trial phase." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:value ] ;
    sh:targetClass odm:ItemData .

odm:ItemDef a sh:NodeShape ;
    sh:closed true ;
    sh:description "An ItemDef describes a type of item that can occur within a study. Item properties include name, datatype, range, or codelist restrictions, and several other properties." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:RangeCheck ;
            sh:description "RangeCheck reference: A RangeCheck defines a constraint on the value of the enclosing item. It represents an expression that evaluates to True when the ItemData value is valid or False when the ItemData value is invalid. The expression is specified using either Comparator and CheckValue or using FormalExpressions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 14 ;
            sh:path odm:rangeCheck ],
        [ sh:class odm:CDISCNotes ;
            sh:description "CDISCNotes reference: Explanatory text for the variable." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 13 ;
            sh:path odm:cDISCNotes ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:description ],
        [ sh:class odm:ImplementationNotes ;
            sh:description "ImplementationNotes reference: Further information, such as rationale and implementation instructions, on how to implement the CRF data collection fields." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 12 ;
            sh:path odm:implementationNotes ],
        [ sh:class odm:Prompt ;
            sh:description "Prompt reference: A prompt text shown to a human user when prompted to provide data for an item on paper or on a screen. The Prompt is a short version of the question." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 10 ;
            sh:path odm:prompt ],
        [ sh:class odm:CRFCompletionInstructions ;
            sh:description "CRFCompletionInstructions reference: Instructions for the clinical site on how to enter collected information on the CRF." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 11 ;
            sh:path odm:cRFCompletionInstructions ],
        [ sh:description "Human readable name for the ItemDef." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Unique identifier for the ItemDef element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Specifies the number of characters allowed for the ItemData/Value when it is represented as a text." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:length ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 17 ;
            sh:path odm:coding ],
        [ sh:description "Display format supports data visualization of numeric float and date values." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:displayFormat ],
        [ sh:description "Specification of the allowable values and the intended use of the corresponding value elements." ;
            sh:in ( "integer" "decimal" "float" "double" "date" "datetime" "time" "text" "string" "URI" "boolean" "hexBinary" "base64Binary" "hexFloat" "base64Float" "partialDate" "partialTime" "partialDatetime" "durationDatetime" "intervalDatetime" "incompleteDatetime" "incompleteDate" "incompleteTime" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:dataType ],
        [ sh:description "Reference to a CommentDef with sponsor provided information related to this ItemDef," ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:commentOID ],
        [ sh:class odm:Question ;
            sh:description "Question reference: A label shown to a human user when prompted to provide data for an item on paper or on a screen." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 9 ;
            sh:path odm:question ],
        [ sh:class odm:CodeListRef ;
            sh:description "CodeListRef reference: A reference to a CodeList definition." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 15 ;
            sh:path odm:codeListRef ],
        [ sh:description "ADaM variable set, e.g. Dose, Analysis Parameter, Treatment Timing." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:variableSet ],
        [ sh:class odm:Alias ;
            sh:description "Alias reference: An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 18 ;
            sh:path odm:alias ],
        [ sh:class odm:ValueListRef ;
            sh:description "ValueListRef reference: The ValueListRef element is the OID of the ValueListDef that contains the valuelist definition associated with the variable. If value-level metadata is required for a variable, a ValueListRef element should be provided as a child element on the ItemDef for the variable definition." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 16 ;
            sh:path odm:valueListRef ],
        [ sh:class odm:Definition ;
            sh:description "A free-text definition of the parameter" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:definition ] ;
    sh:targetClass odm:ItemDef .

odm:ItemGroupDef a sh:NodeShape ;
    sh:closed true ;
    sh:description "An ItemGroupDef describes a type of variable or field grouping that can occur within a study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Description of the level of detail represented by individual records in the ItemGroup" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:structure ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 17 ;
            sh:path odm:coding ],
        [ sh:class odm:WorkflowRef ;
            sh:description "WorkflowRef reference: The WorkflowRef references a workflow definition" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 18 ;
            sh:path odm:workflowRef ],
        [ sh:description "Required for ADaM, SDTM, or SEND if StandardOID is not provided." ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 12 ;
            sh:path odm:isNonStandard ],
        [ sh:description "Unique identifier for the ItemGroupDef element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Purpose of the ItemGroup." ;
            sh:maxCount 1 ;
            sh:order 10 ;
            sh:path odm:purpose ],
        [ sh:class odm:Class ;
            sh:description "Class reference: The Class element identifies which predefined Class within the model applies to the definition of the dataset." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 16 ;
            sh:path odm:classRef ],
        [ sh:class odm:ItemRef ;
            sh:description "ItemRef reference: A reference to an ItemDef as it occurs within a specific ItemGroupDef. The list of ItemRefs identifies the types of items that are allowed to occur within this type of item group." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 23 ;
            sh:path odm:itemRef ],
        [ sh:class odm:Alias ;
            sh:description "Alias reference: An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 20 ;
            sh:path odm:alias ],
        [ sh:description "Reference to a CommentDef with sponsor provided information related to this ItemGroupDef. It allows annotations to the ItemGroup." ;
            sh:maxCount 1 ;
            sh:order 14 ;
            sh:path odm:commentOID ],
        [ sh:class odm:Origin ;
            sh:description "Origin reference: Origin defines the source metadata, where applicable, for ODM ItemRefs or ItemGroupRefs. Origin as a child element replaces the Origin attribute in ODM v1.3 that exists for the ItemDef and ItemGroupDef elements.The Origin element is based on the def:Origin element in Define-XML v2.1 with the Trace-XML v1.0 extension." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 19 ;
            sh:path odm:origin ],
        [ sh:description "Maximum number of repeats." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:repeatingLimit ],
        [ sh:description "Specifies whether this ItemGroupDef is used for non-subject data." ;
            sh:in ( "Yes" "No" ) ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:isReferenceData ],
        [ sh:description "Reference to a Standard element." ;
            sh:maxCount 1 ;
            sh:order 11 ;
            sh:path odm:standardOID ],
        [ sh:description "identifies the type of data structure the ItemGroup represents. Form - a CRF for data collection. Note, ItemGroupDef Type=\"Form\" replaces the ODM v1.x FormDef element. Section - a section within a CRF. Dataset - tabulation, analysis or operational datasets. Concept - defines a biomedical concept." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 9 ;
            sh:path odm:type ],
        [ sh:description "Name of a file containing the ItemGroupData for this ItemGroupDef. The name applies to the object itself rather then providing a mapping to a different object." ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path odm:datasetName ],
        [ sh:class odm:Leaf ;
            sh:description "Leaf reference: Contains the XLink information referenced by DocumentRef or ArchiveLocationID" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 21 ;
            sh:path odm:leaf ],
        [ sh:description "Used to indicate that an ItemGroupDef has no data. May be used at sponsor's discretion or if required by a regulatory authority" ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 13 ;
            sh:path odm:hasNoData ],
        [ sh:description "Human readable name for the ItemGroupDef." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 15 ;
            sh:path odm:description ],
        [ sh:description "Reference to the unique ID of a leaf element that provides the actual location and file name of the data file." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:archiveLocationID ],
        [ sh:description "Identifies the scope or CDISC SDTMIG/SENDIG Domain of the ItemGroup data. The domain applies to the object itself rather then providing a mapping to a different object." ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path odm:domain ],
        [ sh:class odm:ItemGroupRef ;
            sh:description "ItemGroupRef reference: ItemGroupRef references an ItemGroupDef as it occurs within a specific StudyEventDef or ItemGroupDef. The list of ItemGroupRefs identifies the types of item groups that are allowed to occur within this type of studyevent or (nested) item group. The ItemGroupRefs within a single StudyEventDef or ItemGroupDef must not have duplicate ItemGroupOID or OrderNumber attribute values." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 22 ;
            sh:path odm:itemGroupRef ],
        [ sh:description "The Repeating attribute indicates that the ItemGroup may occur repeatedly within the containing element . Simple - the ItemGroup repeats within the containing element and is not bound in any way. Note: It is equivalent to the ODM v1.3.2 case where Repeating=\"Yes\". Dynamic - ItemGroupData repeats based on values in a codelist. There may be multiple occurrences for some codelist items. Static - ItemGroupData repeats based on values in a codelist. Only one occurrence may happen for each codelist item." ;
            sh:in ( "No" "Simple" "Dynamic" "Static" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:repeating ] ;
    sh:targetClass odm:ItemGroupDef .

odm:KeySet a sh:NodeShape ;
    sh:closed true ;
    sh:description "A KeySet references a single entity (e.g., a study, a subject, a study event). Only those attributes needed to specify the particular entity are required, and all others must be omitted (see Section 2.7, Clinical Data Keys)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a StudyEventRepeatKey attribute value for a StudyEventData child element of this ClinicalData/SubjectData/StudyEventData element." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:studyEventRepeatKey ],
        [ sh:description "Reference to an ItemOID attribute for an ItemData child element of this ClinicalData/SubjectData/StudyEventData/ItemGroupData element." ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path odm:itemOID ],
        [ sh:description "References the StudyOID in an ODM/ClinicalData element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyOID ],
        [ sh:description "Reference to an ItemGroupRepeatKey attribute value for an ItemGroupData child element of this ClinicalData/SubjectData/StudyEventData element." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:itemGroupRepeatKey ],
        [ sh:description "Reference to a MetaDataVersionOID attribute value for this ClinicalData element." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:metaDataVersionOID ],
        [ sh:description "Reference to a SubjectKey attribute value for a SubjectData child element of this ClinicalData element." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:subjectKey ],
        [ sh:description "Reference to an ItemGroupOID attribute value for an ItemGroupData child element of this ClinicalData/SubjectData/StudyEventData element." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:itemGroupOID ],
        [ sh:description "Reference to a StudyEventOID attribute value for a StudyEventData child element of this ClinicalData/SubjectData element." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:studyEventOID ] ;
    sh:targetClass odm:KeySet .

odm:LegalReason a sh:NodeShape ;
    sh:closed true ;
    sh:description "The responsibility statement associated with a signature (e.g., \"The signer accepts responsibility for the accuracy of this data.\")." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:LegalReason .

odm:Location a sh:NodeShape ;
    sh:closed true ;
    sh:description "A physical location associated with data collection and/or treatment of subjects." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Unique identifier" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:Query ;
            sh:description "Query reference: The Query element represents a request for clarification on a data item collected for a clinical trial, specifically a request from a sponsor or sponsor’s representative to an investigator to resolve an error or inconsistency discovered during data review. Queries can be created manually by individuals such as site monitors or data managers or automatically by systems. The full text of the Query exists in the Value child element. The optional Name attribute provide the means to provide a short identifier that can be included in listing or user interfaces." ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path odm:query ],
        [ sh:description "Human-readable identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:MetaDataVersionRef ;
            sh:description "MetaDataVersionRef reference: A reference to a MetaDataVersion used at the containing Location. The EffectiveDate reflects the possibility that the metadata may change over the course of the study." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:metaDataVersionRef ],
        [ sh:description "Reference to an organization." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:organizationOID ],
        [ sh:description "Specifies the role of this location in the study." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:role ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:description ],
        [ sh:class odm:Telecom ;
            sh:description "Telecom reference: The telecommunication contacts points of a user, a location, or an organization. The Type attribute designates the type of contact." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:telecom ],
        [ sh:class odm:Address ;
            sh:description "Address reference: The postal address for a user, location, or organization." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:address ] ;
    sh:targetClass odm:Location .

odm:Meaning a sh:NodeShape ;
    sh:closed true ;
    sh:description "A short name or description for this signature. It should reflect the context of the signature and/or the text that appears when the signature is applied in the user interface." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:Meaning .

odm:MetaDataVersion a sh:NodeShape ;
    sh:closed true ;
    sh:description "The metadata for a study is defined in a series of MetaDataVersion elements. Through this mechanism (multiple MetaDataVersion elements), the model supports the incremental deployment of \"mid-stream study changes,\" and thus can handle a situation where multiple versions of the metadata are being used simultaneously (e.g., due to delays in IRB approval at various sites)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "MetaDataVersion name." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:AnnotatedCRF ;
            sh:description "AnnotatedCRF reference: An Annotated Case Report Form (CRF) is a Portable File Format (PDF) document that provides the mapping of data collection fields to the variables or discrete variable values contained within the datasets." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:annotatedCRF ],
        [ sh:class odm:Leaf ;
            sh:description "Leaf reference: Contains the XLink information referenced by DocumentRef or ArchiveLocationID" ;
            sh:nodeKind sh:IRI ;
            sh:order 20 ;
            sh:path odm:leaf ],
        [ sh:class odm:Include ;
            sh:description "Include reference: The Include metadata element allows a reference to a prior metadata version." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:include ],
        [ sh:class odm:ValueListDef ;
            sh:description "ValueListDef reference: The following table specifies the XML structure for valuelist metadata. The ValueListDef element contains ItemRef elements that reference ItemDef elements that provide the value-level metadata details" ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path odm:valueListDef ],
        [ sh:class odm:SupplementalDoc ;
            sh:description "SupplementalDoc reference: Supplemental data definitions" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:supplementalDoc ],
        [ sh:class odm:WorkflowDef ;
            sh:description "WorkflowDef reference: A WorkflowDef defines an automated workflow for a study." ;
            sh:nodeKind sh:IRI ;
            sh:order 11 ;
            sh:path odm:workflowDef ],
        [ sh:class odm:Protocol ;
            sh:description "Protocol reference: The Protocol element lists the kinds of study events that can occur within a specific version of a study. All clinical data must occur within one of these study events." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 10 ;
            sh:path odm:protocol ],
        [ sh:description "Reference to a CommentDef element." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:commentOID ],
        [ sh:class odm:Standards ;
            sh:description "Standards reference: The Standards element provides a container for the list of Standard elements referenced in the MetaDataVersion for the Study.." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:standards ],
        [ sh:class odm:CodeList ;
            sh:description "CodeList reference: Defines a discrete set of permitted values for an item, or provides a reference to a codelist or dictionary maintained by an external organization via the Coding element, or a combination of both. Examples provided under Coding." ;
            sh:nodeKind sh:IRI ;
            sh:order 16 ;
            sh:path odm:codeList ],
        [ sh:class odm:MethodDef ;
            sh:description "MethodDef reference: A MethodDef defines how a data value can be obtained from a collection of other data values." ;
            sh:nodeKind sh:IRI ;
            sh:order 18 ;
            sh:path odm:methodDef ],
        [ sh:description "Unique identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:StudyEventGroupDef ;
            sh:description "StudyEventGroupDef reference: StudyEventGroupDef is a study building block that groups a number of smaller building blocks, which can themselves be StudyEventGroups or StudyEvents. It thus allows nesting of building blocks." ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path odm:studyEventGroupDef ],
        [ sh:class odm:ItemGroupDef ;
            sh:description "ItemGroupDef reference: An ItemGroupDef describes a type of variable or field grouping that can occur within a study." ;
            sh:nodeKind sh:IRI ;
            sh:order 14 ;
            sh:path odm:itemGroupDef ],
        [ sh:class odm:ConditionDef ;
            sh:description "ConditionDef reference: A ConditionDef defines a boolean condition." ;
            sh:nodeKind sh:IRI ;
            sh:order 17 ;
            sh:path odm:conditionDef ],
        [ sh:class odm:ItemDef ;
            sh:description "ItemDef reference: An ItemDef describes a type of item that can occur within a study. Item properties include name, datatype, range, or codelist restrictions, and several other properties." ;
            sh:nodeKind sh:IRI ;
            sh:order 15 ;
            sh:path odm:itemDef ],
        [ sh:class odm:WhereClauseDef ;
            sh:description "WhereClauseDef reference: The WhereClauseDef element specifies a condition." ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path odm:whereClauseDef ],
        [ sh:class odm:CommentDef ;
            sh:description "CommentDef reference: The Comment element allows referencing short comments self-contained in the XML document or long comments normally included in external documents. For comments included in external documents, the reference could include specific pages of a document where the comments are included." ;
            sh:nodeKind sh:IRI ;
            sh:order 19 ;
            sh:path odm:commentDef ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:description ],
        [ sh:class odm:StudyEventDef ;
            sh:description "StudyEventDef reference: StudyEventDef represents the definition of an activity in a study where data is collected. For example, a study event may represent a set of item groups that represent data collection instruments to be completed for a subject during a visit in a study. The visit occurs as part of a study workflow, and the workflow is referenced in the study event." ;
            sh:nodeKind sh:IRI ;
            sh:order 13 ;
            sh:path odm:studyEventDef ] ;
    sh:targetClass odm:MetaDataVersion .

odm:MetaDataVersionRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reference to a MetaDataVersion used at the containing Location. The EffectiveDate reflects the possibility that the metadata may change over the course of the study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "References the Study that uses this metadata version." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyOID ],
        [ sh:description "References the MetaDataVersion (within the above Study)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:metaDataVersionOID ],
        [ sh:description "Datetime stamp when this MetaDataVersion was published at this location." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:effectiveDate ] ;
    sh:targetClass odm:MetaDataVersionRef .

odm:MethodDef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A MethodDef defines how a data value can be obtained from a collection of other data values." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Human readable name for the method." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Reference to a CommentDef with information related to this MethodDef." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:commentOID ],
        [ sh:class odm:FormalExpression ;
            sh:description "FormalExpression reference: A FormalExpression used within a ConditionDef or a RangeCheck must evaluate to True or False. A FormalExpression referenced within a MethodDef having Type Imputation, Computation, or Transpose must evaluate to the correct DataType for an Item that may be imputed or computed using the Method. A FormalExpression gets parameter and return value definitions from the MethodSignature element. The data types in the MethodSignature parameters and return values must match the corresponding data types in the FormalExpression." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:formalExpression ],
        [ sh:class odm:DocumentRef ;
            sh:description "The DocumentRef element is a container for page references in a PDF file." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:documentRef ],
        [ sh:class odm:Alias ;
            sh:description "Alias reference: An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:alias ],
        [ sh:class odm:MethodSignature ;
            sh:description "MethodSignature reference: A MethodSignature defines the parameters and return values for a method. The MethodSignature improves traceability while enhancing the ability for automation engines to execute a MethodDef's FormalExpression. Most Methods use one or more input parameters and return one or more values." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:methodSignature ],
        [ sh:description "Computation: derivation involving one or more variables. Imputation: derivation based on other observations. Transpose: conversion of rows to columns or vice versa. Preload: retrieve and populate data from an external source." ;
            sh:in ( "Computation" "Imputation" "Preload" "Transpose" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:type ],
        [ sh:description "Unique identifer for the MethodDef element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:description ] ;
    sh:targetClass odm:MethodDef .

odm:Organization a sh:NodeShape ;
    sh:closed true ;
    sh:description "An organization can reference a parent organization. Users may be associated with an Organization. An Organization may be associated with a Location. A User, Location, or Organization may have an address." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Telecom ;
            sh:description "Telecom reference: The telecommunication contacts points of a user, a location, or an organization. The Type attribute designates the type of contact." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:telecom ],
        [ sh:description "Role of the organization in the current study." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:role ],
        [ sh:description "Categorization of organizations associated with clinical studies." ;
            sh:in ( "Sponsor" "Site" "CRO" "Lab" "Other" "TechnologyProvider" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path odm:type ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:description ],
        [ sh:description "Unique identifier for the organization." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Reference to a location where data is collected and/or study subjects are treated." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:locationOID ],
        [ sh:class odm:Address ;
            sh:description "Address reference: The postal address for a user, location, or organization." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:address ],
