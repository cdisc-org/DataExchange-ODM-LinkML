            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:endOID ] ;
    sh:targetClass odm:WorkflowEnd .

odm:WorkflowStart a sh:NodeShape ;
    sh:closed true ;
    sh:description "WorkflowStart references a structural element that begins the automated workflow." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to the definition of the structural element that starts the workflow. It may be a StudyEventGroupDef, StudyEventDef, ItemGroupDef, or ItemDef element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:startOID ] ;
    sh:targetClass odm:WorkflowStart .

odm:Criterion a sh:NodeShape ;
    sh:closed true ;
