odm:ItemGroupRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "ItemGroupRef references an ItemGroupDef as it occurs within a specific StudyEventDef or ItemGroupDef. The list of ItemGroupRefs identifies the types of item groups that are allowed to occur within this type of studyevent or (nested) item group. The ItemGroupRefs within a single StudyEventDef or ItemGroupDef must not have duplicate ItemGroupOID or OrderNumber attribute values." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a ConditionDef If the CollectionExceptionConditionOID attribute is provided, the ConditionDef it references describes the circumstances under which data for this ItemGroup should not be collected." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:collectionExceptionConditionOID ],
        [ sh:description "Reference to the ItemGroupDef ." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:itemGroupOID ],
        [ sh:description "Indicates the order in which this ItemGroup appears in Metadata displays or data entry applications. The OrderNumber attribute provides an ordering on the ItemGroupDefs (within StudyEventDef or ItemGroupDef) for use whenever a list of ItemGroupDefs is presented to a user. Order of execution is preferably defined in a workflow (see Section 3.2.2.1.8, WorkflowDef ) but when used without a workflow, may be used the define the order in which data entry forms are presented in an application UI." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:orderNumber ],
        [ sh:description "Reference to a MethodDef that will provide one or more data rows as output. The MethodDef is used to prepopulate items" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:methodOID ],
        [ sh:description "The Mandatory flag indicates that the clinical data for an instance of the containing event or ItemGroup would be incomplete without an instance of this type of ItemGroup. ODM clinical data files that are incomplete in this sense may be considered incomplete for study review and analysis purposes." ;
            sh:in ( "Yes" "No" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path odm:mandatory ] ;
    sh:targetClass odm:ItemGroupRef .

odm:ItemRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reference to an ItemDef as it occurs within a specific ItemGroupDef. The list of ItemRefs identifies the types of items that are allowed to occur within this type of item group." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Indicates that this item is a key for the enclosing element. It also provides an ordering for the keys." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:keySequence ],
        [ sh:description "CDASH, ADaM, SDTM, and SEND Core designations." ;
            sh:maxCount 1 ;
            sh:order 10 ;
            sh:path odm:core ],
        [ sh:description "Reference to a sibling ItemRef element that represents the unit specification for this ItemRef (defines the associated units variable)." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:unitsItemOID ],
        [ sh:description "Indicator of whether this ItemGroup must appear in the study clinical data for each subject per the study protocol. The Mandatory flag indicates that the clinical data for an instance of the containing element would be incomplete without an instance of this type of item. ODM clinical data files that are incomplete in this sense may be considered incomplete for study review and analysis purposes." ;
            sh:in ( "Yes" "No" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 13 ;
            sh:path odm:mandatory ],
        [ sh:description "Flag to indicate that the Item represents \"other\" content added to an ItemGroup." ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path odm:other ],
        [ sh:class odm:Origin ;
            sh:description "Origin reference: Origin defines the source metadata, where applicable, for ODM ItemRefs or ItemGroupRefs. Origin as a child element replaces the Origin attribute in ODM v1.3 that exists for the ItemDef and ItemGroupDef elements.The Origin element is based on the def:Origin element in Define-XML v2.1 with the Trace-XML v1.0 extension." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 15 ;
            sh:path odm:origin ],
        [ sh:description "Reference to a MethodDef used to derive the value of this item." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:methodOID ],
        [ sh:description "Indicates that the item serves as the item over which repeats are to be performed." ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:repeat ],
        [ sh:class odm:WhereClauseRef ;
            sh:description "WhereClauseRef reference: The WhereClauseRef references the WhereClauseDef element that describes the conditions under which the variable values are defined by the referenced ItemDef." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 16 ;
            sh:path odm:whereClauseRef ],
        [ sh:description "A non-standard variable is one that is not defined in the SDTMIG or the underlying SDTM model of a standard referenced within the ODM document. Permitted for any ItemRef that represents a non-standard variable in an SDTM or SEND dataset when the ODM Context attribute value is \"Other\" Is not used with an ItemRef that represents a value level metadata for an SDTMIG SUPPQUAL dataset Has no meaning for ADaM, so shall not be used for ADaM variables" ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:isNonStandard ],
        [ sh:description "Indicates the order in which this ItemRef appears in Metadata displays or data entry applications. The OrderNumbers provide an ordering on the Items (within a containing element) for use whenever a list of Items is presented to a user. They do not imply anything about event scheduling, time ordering, or data correctness." ;
            sh:maxCount 1 ;
            sh:order 12 ;
            sh:path odm:orderNumber ],
        [ sh:description "Used to indicate that an ItemRef that represent a dataset's variable has no data. Note that variables refer to both standard and non-standard/ supplemental qualifiers variables (/ODM/Study/MetaDataVersion/ItemGroupDef/ItemRef or /ODM/Study/MetaDataVersion/ValueListDef/ItemRef)." ;
            sh:in [ rdf:rest () ] ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:hasNoData ],
        [ sh:description "Prefill value or a default value for a field that is automatically populated." ;
            sh:maxCount 1 ;
            sh:order 11 ;
            sh:path odm:preSpecifiedValue ],
        [ sh:description "Reference to a CodeList that defines the allowable values of Role for the Study." ;
            sh:maxCount 1 ;
            sh:order 9 ;
            sh:path odm:roleCodeListOID ],
        [ sh:description "Reference to the ItemDef ." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:itemOID ],
        [ sh:description "Reference to a ConditionDef that defines the circumstances under which collection of the Item may be omitted." ;
            sh:maxCount 1 ;
            sh:order 14 ;
            sh:path odm:collectionExceptionConditionOID ],
        [ sh:description "The Role for the referenced ItemDef. The Role attribute provides a single role name describing the use of this data item. If the Role is defined by a standard terminology, RoleCodeListOID may be used to reference a CodeList that defines the full set roles from which the Role attribute value is to be taken." ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path odm:role ] ;
    sh:targetClass odm:ItemRef .

odm:Leaf a sh:NodeShape ;
    sh:closed true ;
    sh:description "Contains the XLink information referenced by DocumentRef or ArchiveLocationID" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "URL that can be used to identify the location of a document or dataset file relative to the folder containing the ODM file. If the file is not located in the same folder as the ODM file, a relative file path should be included. Business Rule: For regulatory submissions to the FDA, the locations specified have to conform to locations allowed in the eCTD, in the Technical Conformance Guide for the relevant regulatory authority and the study data specifications." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:href ],
        [ sh:class odm:Title ;
            sh:description "Text with the label for the document or dataset." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:title ],
        [ sh:description "Unique ID for the Leaf. See Section 2.13, Element Identifiers and References , for OID considerations. Business Rule: Leaf ID attributes must be unique within the ODM document (i.e., there can be no 2 Leaf elements with the same ID attribute)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:iD ] ;
    sh:targetClass odm:Leaf .

odm:LocationRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reference to the user's physical location." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a Location definition." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:locationOID ] ;
    sh:targetClass odm:LocationRef .

odm:Origin a sh:NodeShape ;
    sh:closed true ;
    sh:description "Origin defines the source metadata, where applicable, for ODM ItemRefs or ItemGroupRefs. Origin as a child element replaces the Origin attribute in ODM v1.3 that exists for the ItemDef and ItemGroupDef elements.The Origin element is based on the def:Origin element in Define-XML v2.1 with the Trace-XML v1.0 extension." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:DocumentRef ;
            sh:description "The DocumentRef element is a container for page references in a PDF file." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:documentRef ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:coding ],
        [ sh:description "Identifies how the clinical data values were obtained." ;
            sh:in ( <nci:ExtCodeID:C170547> <nci:ExtCodeID:C170548> <nci:ExtCodeID:C170549> "EHR" <nci:ExtCodeID:C126101> <nci:ExtCodeID:C17649> <nci:ExtCodeID:C170550> <nci:ExtCodeID:C170551> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:type ],
        [ sh:class odm:SourceItems ;
            sh:description "SourceItems reference: Identifies source items as needed to support automated data capture and end-to-end traceability." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:sourceItems ],
        [ sh:description "Indicates the party responsible for the data's origin type." ;
            sh:in ( <nci:ExtCodeID:C25936> <nci:ExtCodeID:C70793> <nci:ExtCodeID:C41189> <nci:ExtCodeID:C68608> ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:source ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:description ] ;
    sh:targetClass odm:Origin .

odm:RangeCheck a sh:NodeShape ;
    sh:closed true ;
    sh:description "A RangeCheck defines a constraint on the value of the enclosing item. It represents an expression that evaluates to True when the ItemData value is valid or False when the ItemData value is invalid. The expression is specified using either Comparator and CheckValue or using FormalExpressions." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Type of range check. Soft indicates that a warning occurs when the RangeCheck fails. Hard indicates that an error occurs when the RangeCheck fails." ;
            sh:in ( "Soft" "Hard" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:softHard ],
        [ sh:description "Comparison operator used to compare the item and value(s)." ;
            sh:in ( "LT" "LE" "GT" "GE" "EQ" "NE" "IN" "NOTIN" ) ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:comparator ],
        [ sh:class odm:ErrorMessage ;
            sh:description "ErrorMessage reference: Error message provided to user when the range check fails." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:errorMessage ],
        [ sh:class odm:MethodSignature ;
            sh:description "MethodSignature reference: A MethodSignature defines the parameters and return values for a method. The MethodSignature improves traceability while enhancing the ability for automation engines to execute a MethodDef's FormalExpression. Most Methods use one or more input parameters and return one or more values." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:methodSignature ],
        [ sh:class odm:CheckValue ;
            sh:description "CheckValue reference: A comparison value used in a range check." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:checkValue ],
        [ sh:class odm:FormalExpression ;
            sh:description "FormalExpression reference: A FormalExpression used within a ConditionDef or a RangeCheck must evaluate to True or False. A FormalExpression referenced within a MethodDef having Type Imputation, Computation, or Transpose must evaluate to the correct DataType for an Item that may be imputed or computed using the Method. A FormalExpression gets parameter and return value definitions from the MethodSignature element. The data types in the MethodSignature parameters and return values must match the corresponding data types in the FormalExpression." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:formalExpression ],
        [ sh:description "Identifies a variable to compare against." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:itemOID ] ;
    sh:targetClass odm:RangeCheck .

odm:StudyEndPointRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reference to a StudyEndPoint as it occurs within a specific StudyObjective." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Indicates the order in which this StudyEndPointRef appears in Metadata displays or data entry applications." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:orderNumber ],
        [ sh:description "Reference to the StudyEndPoint ." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyEndPointOID ] ;
    sh:targetClass odm:StudyEndPointRef .

odm:StudyEventGroupRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "This element references a StudyEventGroupDef as it occurs within a specific version of a study. The list of StudyEventGroupRefs identifies the types of study group events that are allowed to occur within the study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:description ],
        [ sh:description "Reference to the StudyEventGroupDef ." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyEventGroupOID ],
        [ sh:description "Indicates the order in which this StudyEventGroup appears in Metadata displays or data entry applications." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:orderNumber ],
        [ sh:description "Indicator of whether this StudyEventGroup must appear in the study clinical data for each subject per the study protocol." ;
            sh:in ( "Yes" "No" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:mandatory ],
        [ sh:description "Reference to a ConditionDef" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:collectionExceptionConditionOID ] ;
    sh:targetClass odm:StudyEventGroupRef .

odm:UserRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "A reference to information about a specific user of a clinical data collection or data management system." ;
