        [ sh:description "Unique identifier for the workflow." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:Transition ;
            sh:description "Transition reference: A Transition defines a link between 2 structural elements in a workflow. When the execution of the transition is dependent upon a timing constraint that is either directly defined or calculated, a TransitionTimingConstraint must be defined, referencing the current Transition." ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path odm:transition ] ;
    sh:targetClass odm:WorkflowDef .

odm:WorkflowEnd a sh:NodeShape ;
    sh:closed true ;
    sh:description "A WorkflowEnd references a structural element with which the workflows ends." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:content ],
        [ sh:description "Reference to the definition of the structural element that ends the workflow. It may be a StudyEventGroupDef, StudyEventDef, ItemGroupDef, or ItemDef element." ;
