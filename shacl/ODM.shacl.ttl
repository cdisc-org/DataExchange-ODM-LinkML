odm:Protocol a sh:NodeShape ;
    sh:closed true ;
    sh:description "The Protocol element lists the kinds of study events that can occur within a specific version of a study. All clinical data must occur within one of these study events." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Alias ;
            sh:description "Alias reference: An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 14 ;
            sh:path odm:alias ],
        [ sh:class odm:StudySummary ;
            sh:description "StudySummary reference: The StudyParameter element allows to provide a set of study design parameters such as anticipated number of subjects, minimum and maximum age of the participants, or planned number of arms." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:studySummary ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:description ],
        [ sh:class odm:StudyIndications ;
            sh:description "StudyIndications reference: StudyIndications is a container element for individual StudyIndication elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:studyIndications ],
        [ sh:class odm:StudyTargetPopulation ;
            sh:description "StudyTargetPopulation reference: The StudyTargetPopulation describes the population targeted for the clinical study." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path odm:studyTargetPopulation ],
        [ sh:class odm:StudyInterventions ;
            sh:description "StudyInterventions reference: The StudyInterventions element is a container element for individual StudyIntervention elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:studyInterventions ],
        [ sh:class odm:TrialPhase ;
            sh:description "TrialPhase reference: The TrialPhase element designates the phase of the study in the clinical trial." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:trialPhase ],
        [ sh:class odm:InclusionExclusionCriteria ;
            sh:description "InclusionExclusionCriteria reference: The InclusionExclusionCriteria element can contain 2 lists of Criterion elements, represented by the 2 elements InclusionCriteria and ExclusionCriteria. Together, these criteria determine the eligibility of a subject for the study. The actual condition to be evaluated is contained in an ODM ConditionDef, which is referenced by each Criterion‟s ConditionOID attribute." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 11 ;
            sh:path odm:inclusionExclusionCriteria ],
        [ sh:class odm:StudyStructure ;
            sh:description "StudyStructure reference: The StudyStructure element describes the general structure of a clinical study with arms, epochs, and workflows." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:studyStructure ],
        [ sh:class odm:StudyTimings ;
            sh:description "StudyTimings reference: The StudyTimings element is a container element for individual StudyTiming elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:studyTimings ],
        [ sh:class odm:StudyEstimands ;
            sh:description "StudyEstimands reference: StudyEstimands is a container element for individual StudyEstimand elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 10 ;
            sh:path odm:studyEstimands ],
        [ sh:class odm:StudyObjectives ;
            sh:description "StudyObjectives reference: The StudyObjectives is a container element for individual StudyObjective elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:studyObjectives ],
        [ sh:class odm:StudyEventGroupRef ;
            sh:description "StudyEventGroupRef reference: This element references a StudyEventGroupDef as it occurs within a specific version of a study. The list of StudyEventGroupRefs identifies the types of study group events that are allowed to occur within the study." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 12 ;
            sh:path odm:studyEventGroupRef ],
        [ sh:class odm:WorkflowRef ;
            sh:description "WorkflowRef reference: The WorkflowRef references a workflow definition" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 13 ;
            sh:path odm:workflowRef ],
        [ sh:class odm:StudyEndPoints ;
            sh:description "StudyEndPoints reference: The StudyEndPoints element is a container element for individual StudyEndPoint elements." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:studyEndPoints ] ;
    sh:targetClass odm:Protocol .

odm:Question a sh:NodeShape ;
    sh:closed true ;
