            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path odm:status ],
        [ sh:description "The name of the publishing set that contains the published standard." ;
            sh:in ( <nci:ExtCodeID:C180548> <nci:ExtCodeID:C180549> <nci:ExtCodeID:C180550> <nci:ExtCodeID:C180551> <nci:ExtCodeID:C180552> ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:publishingSet ],
        [ sh:description "The type of standard." ;
            sh:in ( <nci:ExtCodeID:C163415> <nci:ExtCodeID:C170454> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:type ],
        [ sh:description "Version of the external standard to which the data conforms." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path odm:version ],
        [ sh:description "Reference to the unique ID of a CommentDef element that contains the comment for the Standard element Business Rule: Must match the OID of a CommentDef in the same MetaDataVersion" ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:commentOID ],
        [ sh:description "Unique identifier for the standard." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Name of the external standard to which the data conforms." ;
            sh:in ( <nci:ExtCodeID:C170552> <nci:ExtCodeID:C163415> <nci:ExtCodeID:C170455> <nci:ExtCodeID:C170553> <nci:ExtCodeID:C170554> <nci:ExtCodeID:C170456> <nci:ExtCodeID:C181230> <nci:ExtCodeID:C170556> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ] ;
    sh:targetClass odm:Standard .

odm:Standards a sh:NodeShape ;
    sh:closed true ;
    sh:description "The Standards element provides a container for the list of Standard elements referenced in the MetaDataVersion for the Study.." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Standard ;
            sh:description "Definition of a standard referenced in the Define-XML document." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:standard ] ;
    sh:targetClass odm:Standards .

odm:StateProv a sh:NodeShape ;
    sh:closed true ;
    sh:description "The state or province name part of a user's postal address." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:StateProv .

odm:StreetName a sh:NodeShape ;
    sh:closed true ;
    sh:description "The street name part of a user's postal address." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:StreetName .

odm:Study a sh:NodeShape ;
    sh:closed true ;
    sh:description "This element collects static structural information about an individual study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Sponsoring organization's internal name for the study. If no internal name is available, the value is expected to be the same value as ProtocolName." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:studyName ],
        [ sh:description "Unique identifier for the study." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:MetaDataVersion ;
            sh:description "MetaDataVersion reference: The metadata for a study is defined in a series of MetaDataVersion elements. Through this mechanism (multiple MetaDataVersion elements), the model supports the incremental deployment of \"mid-stream study changes,\" and thus can handle a situation where multiple versions of the metadata are being used simultaneously (e.g., due to delays in IRB approval at various sites)." ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path odm:metaDataVersion ],
        [ sh:description "Identifier for the specific version of the study in the source system that the enclosed Study element metadata refers to." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:versionID ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:description ],
        [ sh:description "Protocol identifier or protocol number assigned to the study. It is used by the regulatory authority or clinical trial registry." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:protocolName ],
        [ sh:description "Represents the workflow status for the version of the study with content included in the enclosed Study element metadata. Status values can be different in each system and may have specific meaning within each system, e.g. a \"Production\" version of a study in a particular source system may be non-editable and editable in a different source system. If no VersionID is provided, Status refers to the status of the Study as a whole." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:status ],
        [ sh:description "Short descriptive label for the version of the study, e.g. \"Initial go live\" when VersionID = \"<study version ID for Initial go live>\". VersionName may be provided when a VersionID is provided." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:versionName ] ;
    sh:targetClass odm:Study .

odm:StudyEndPoint a sh:NodeShape ;
    sh:closed true ;
    sh:description "A study end point reflects an outcome measure of interest that is statistically analyzed to address a particular research question for the study. It typically specifies the type of assessments made; the timing of those assessments; the assessment tools used; and other details, as applicable, such as how multiple assessments within an individual are to be combined." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The type of end point. Simple: measures the change of a single outcome that is meaningful in the context of the disease being studied. Humane: the point at which pain and/or distress is terminated, minimized, or reduced. Surrogate: a measure of effect of a specific treatment that may correlate with a real clinical endpoint but does not necessarily have a guaranteed relationship (e.g., a biomarker). Combined: end point that is a combination of several measures. Example: A heart attack study may report the incidence of the combined endpoint of chest pain, myocardial infarction, or death." ;
            sh:in ( "Simple" "Humane" "Surrogate" "Composite" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:type ],
        [ sh:description "Unique identifier for the StudyEndPoint element" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:FormalExpression ;
            sh:description "FormalExpression reference: A FormalExpression used within a ConditionDef or a RangeCheck must evaluate to True or False. A FormalExpression referenced within a MethodDef having Type Imputation, Computation, or Transpose must evaluate to the correct DataType for an Item that may be imputed or computed using the Method. A FormalExpression gets parameter and return value definitions from the MethodSignature element. The data types in the MethodSignature parameters and return values must match the corresponding data types in the FormalExpression." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:formalExpression ],
        [ sh:description "Level for the Study Endpoint. Primary endpoint(s) are typically efficacy measures that address the main research question [1] Secondary endpoints are generally not sufficient to influence decision-making alone, but may support the claim of efficacy by demonstrating additional effects or by supporting a causal mechanism. [2] Exploratory endpoints (where nominated) typically capture outcomes that occur less frequently or which may be useful for exploring novel hypotheses. [1]" ;
            sh:in ( "Primary" "Secondary" "Exploratory" ) ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:level ],
        [ sh:description "Human readable identifier for the StudyEndPoint element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:description ] ;
    sh:targetClass odm:StudyEndPoint .

odm:StudyEndPoints a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyEndPoints element is a container element for individual StudyEndPoint elements." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:StudyEndPoint ;
            sh:description "StudyEndPoint reference: A study end point reflects an outcome measure of interest that is statistically analyzed to address a particular research question for the study. It typically specifies the type of assessments made; the timing of those assessments; the assessment tools used; and other details, as applicable, such as how multiple assessments within an individual are to be combined." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:studyEndPoint ] ;
    sh:targetClass odm:StudyEndPoints .

odm:StudyEstimand a sh:NodeShape ;
    sh:closed true ;
    sh:description "A precise description of the treatment effect reflecting the clinical question posed by a given clinical trial objective. It summarises at a population level what the outcomes would be in the same patients under different treatment conditions being compared." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:SummaryMeasure ;
            sh:description "SummaryMeasure reference: The SummaryMeasure element describes a summary measure for an estimand (e.g., proportion of patients with an improvement)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:summaryMeasure ],
        [ sh:class odm:StudyTargetPopulationRef ;
            sh:description "StudyTargetPopulationRef reference: The StudyTargetPopulationRef references a StudyTargetPopulation to which the estimand applies." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:studyTargetPopulationRef ],
        [ sh:description "Defined Level for the Study Estimand" ;
            sh:in ( "Primary" "Secondary" "Exploratory" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:level ],
        [ sh:class odm:StudyEndPointRef ;
            sh:description "StudyEndPointRef reference: A reference to a StudyEndPoint as it occurs within a specific StudyObjective." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:studyEndPointRef ],
        [ sh:description "Unique identifier for the StudyEstimand element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:description ],
        [ sh:description "Human readable name for the Study Estimand." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:StudyInterventionRef ;
            sh:description "StudyInterventionRef reference: The StudyInterventionRef references an intervention that is taken as the treatment for the estimand." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:studyInterventionRef ],
        [ sh:class odm:IntercurrentEvent ;
            sh:description "IntercurrentEvent reference: The IntercurrentEvent element describes an intercurrent event for an estimand (e.g., treatment discontinuation)." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:intercurrentEvent ] ;
    sh:targetClass odm:StudyEstimand .

odm:StudyEstimands a sh:NodeShape ;
    sh:closed true ;
    sh:description "StudyEstimands is a container element for individual StudyEstimand elements." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:StudyEstimand ;
            sh:description "StudyEstimand reference: A precise description of the treatment effect reflecting the clinical question posed by a given clinical trial objective. It summarises at a population level what the outcomes would be in the same patients under different treatment conditions being compared." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:studyEstimand ] ;
    sh:targetClass odm:StudyEstimands .

odm:StudyEventData a sh:NodeShape ;
    sh:closed true ;
    sh:description "Clinical data for a study event (visit). The model supports repeating study events (e.g., when the same set of information is collected for a series of patient visits)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Annotation ;
            sh:description "Annotation reference: A general note about clinical data. If an annotation has both a comment and flags, the flags should be related to the comment." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path odm:annotation ],
        [ sh:description "Reference to the StudyEventDef . The StudyEventOID and StudyEventRepeatKey are used together to identify a particular study event. This pair of values uniquely identifies a StudyEvent within the containing subject. The StudyEventRepeatKey is present if and only if the StudyEventDef is repeating." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyEventOID ],
        [ sh:class odm:Signature ;
            sh:description "Signature reference: An electronic signature applies to a collection of clinical data. This indicates that some user accepts legal responsibility for that data. See 21 CFR Part 11. The signature identifies the person signing, the location of signing, the signature meaning (via the referenced SignatureDef), the date and time of signing, and (in the case of a digital signature) an encrypted hash of the included data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path odm:signature ],
        [ sh:description "Identifies the transaction type when /ODM/@FileType is Transactional and there is no child element." ;
            sh:in ( "Insert" "Update" "Remove" "Upsert" "Context" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:transactionType ],
        [ sh:description "A key used to distinguish between repeats of the same type of study event for a single subject." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:studyEventRepeatKey ],
        [ sh:class odm:Query ;
            sh:description "Query reference: The Query element represents a request for clarification on a data item collected for a clinical trial, specifically a request from a sponsor or sponsor’s representative to an investigator to resolve an error or inconsistency discovered during data review. Queries can be created manually by individuals such as site monitors or data managers or automatically by systems. The full text of the Query exists in the Value child element. The optional Name attribute provide the means to provide a short identifier that can be included in listing or user interfaces." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path odm:query ],
        [ sh:class odm:ItemGroupData ;
            sh:description "ItemGroupData reference: Clinical data corresponding to an ItemGroupRef defined in the active MetaDataVersion." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:itemGroupData ],
        [ sh:class odm:AuditRecord ;
            sh:description "AuditRecord reference: An AuditRecord carries information pertaining to the creation, deletion, or modification of clinical data. This information includes who performed that action, and where, when, and why that action was performed.AuditRecord information describes a change to clinical data, but is not itself clinical data. The value of some clinical data can always be changed by a subsequent transaction, but history cannot be changed, only added to." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:auditRecord ] ;
    sh:targetClass odm:StudyEventData .

odm:StudyEventDef a sh:NodeShape ;
    sh:closed true ;
    sh:description "StudyEventDef represents the definition of an activity in a study where data is collected. For example, a study event may represent a set of item groups that represent data collection instruments to be completed for a subject during a visit in a study. The visit occurs as part of a study workflow, and the workflow is referenced in the study event." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:ItemGroupRef ;
            sh:description "ItemGroupRef reference: ItemGroupRef references an ItemGroupDef as it occurs within a specific StudyEventDef or ItemGroupDef. The list of ItemGroupRefs identifies the types of item groups that are allowed to occur within this type of studyevent or (nested) item group. The ItemGroupRefs within a single StudyEventDef or ItemGroupDef must not have duplicate ItemGroupOID or OrderNumber attribute values." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:itemGroupRef ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 9 ;
            sh:path odm:coding ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:description ],
        [ sh:class odm:WorkflowRef ;
            sh:description "WorkflowRef reference: The WorkflowRef references a workflow definition" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:workflowRef ],
        [ sh:description "Specifies the StudyEvent Type. The study protocol document usually specifies the planned set of study subject encounters, as follows: Scheduled - StudyEvent is a planned study event Unscheduled - Unplanned study event (for example an unexpected adverse event that occurs between planned visits. Common - A set of study activities that occur for as part of all subject encounters within a study." ;
            sh:in ( "Scheduled" "Unscheduled" "Common" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path odm:type ],
        [ sh:description "Unique identifier for the StudyEventDef element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "The Repeating flag indicates when this type of study event can occur repeatedly within any given subject. When Repeating is \"Yes\" multiple instances of StudyEventData for this StudyEventDef may be collected for a study subject." ;
            sh:in ( "Yes" "No" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:repeating ],
        [ sh:class odm:Alias ;
            sh:description "Alias reference: An Alias provides an additional name for an element. The Context attribute specifies the application domain in which this additional name is relevant." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 10 ;
            sh:path odm:alias ],
        [ sh:description "Human readable-name for the study event." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Reference to a sponsor comment or external document relevant to this StudyEvent definition." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:commentOID ],
        [ sh:description "The Category attribute is typically used to indicate the study phase appropriate to this type of study event. Examples might include screening, pretreatment, treatment, and follow-up." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:category ] ;
    sh:targetClass odm:StudyEventDef .

odm:StudyEventGroupDef a sh:NodeShape ;
    sh:closed true ;
    sh:description "StudyEventGroupDef is a study building block that groups a number of smaller building blocks, which can themselves be StudyEventGroups or StudyEvents. It thus allows nesting of building blocks." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Unique identifier for the StudyEventGroupDef element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Reference to a CommentDef element defined in the study." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:commentOID ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:coding ],
        [ sh:class odm:WorkflowRef ;
            sh:description "WorkflowRef reference: The WorkflowRef references a workflow definition" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:workflowRef ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:description ],
        [ sh:description "Reference to an Epoch element defined in the study." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:epochOID ],
        [ sh:description "Reference to an Arm element defined in the study." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:armOID ],
        [ sh:description "Human readable identifier for the StudyEventGroupDef element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:StudyEventGroupRef ;
            sh:description "StudyEventGroupRef reference: This element references a StudyEventGroupDef as it occurs within a specific version of a study. The list of StudyEventGroupRefs identifies the types of study group events that are allowed to occur within the study." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:studyEventGroupRef ],
        [ sh:class odm:StudyEventRef ;
            sh:description "StudyEventRef reference: This element references a StudyEventDef as it occurs within a specific version of a study. The list of StudyEventRefs identifies the types of study events that are allowed to occur within the study." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 9 ;
            sh:path odm:studyEventRef ] ;
    sh:targetClass odm:StudyEventGroupDef .

odm:StudyEventRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "This element references a StudyEventDef as it occurs within a specific version of a study. The list of StudyEventRefs identifies the types of study events that are allowed to occur within the study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The Mandatory flag indicates that the clinical data for the containing MetaDataVersion would be incomplete without an instance of this type of Study Event for each subject per the study protocol. ODM clinical data files that are incomplete in this sense may be considered incomplete for study review and analysis purposes." ;
            sh:in ( "Yes" "No" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:mandatory ],
        [ sh:description "Reference to the StudyEventDef ." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyEventOID ],
        [ sh:description "Indicates the order in which this StudyEventDef appears in Metadata displays or data entry applications. OrderNumbers provide an ordering on the StudyEventDefs for use whenever a list of StudyEventDefs is presented to a user. They do not imply anything about event scheduling, time ordering, or data correctness. Order of execution and timing between study events and groups of study events must be defined in WorkflowDef elements. Scheduling will be realized in StudyTiming and its child elements that reference StudyEventGroups." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:orderNumber ],
        [ sh:description "Reference to a ConditionDef If the CollectionExceptionConditionOID attribute is provided, it references a ConditionDef that describes the circumstances under which data for this study event should not be collected." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:collectionExceptionConditionOID ] ;
    sh:targetClass odm:StudyEventRef .

odm:StudyIndication a sh:NodeShape ;
    sh:closed true ;
    sh:description "This element describes a study indication (e.g., condition, disease) for the clinical study. The human-readable description is provided in the Description element. The Coding element can be used to provide a machine-readable code for the indication (e.g., SNOMED-CT code 26929004 for \"Alzheimer's disease\")." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:coding ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:description ],
        [ sh:description "Unique identifier of the version within the XML document." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ] ;
    sh:targetClass odm:StudyIndication .

odm:StudyIndications a sh:NodeShape ;
    sh:closed true ;
    sh:description "StudyIndications is a container element for individual StudyIndication elements." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:StudyIndication ;
            sh:description "StudyIndication reference: This element describes a study indication (e.g., condition, disease) for the clinical study. The human-readable description is provided in the Description element. The Coding element can be used to provide a machine-readable code for the indication (e.g., SNOMED-CT code 26929004 for \"Alzheimer's disease\")." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:studyIndication ] ;
    sh:targetClass odm:StudyIndications .

odm:StudyIntervention a sh:NodeShape ;
    sh:closed true ;
    sh:description "This element describes a study intervention (e.g., medication, treatment, therapy) for the clinical study. The human-readable description is provided in the Description element. The Coding element can be used to provide a machine-readable code for the indication (e.g., ATC M01AE01 code for \"Ibuprofen\" when used as a nonsteroidal anti-inflammatory drug)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:description ],
        [ sh:description "Unique identifier of the version within the XML document." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:coding ] ;
    sh:targetClass odm:StudyIntervention .

odm:StudyInterventionRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyInterventionRef references an intervention that is taken as the treatment for the estimand." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to a StudyIntervention" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyInterventionOID ] ;
    sh:targetClass odm:StudyInterventionRef .

odm:StudyInterventions a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyInterventions element is a container element for individual StudyIntervention elements." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:StudyIntervention ;
            sh:description "StudyIntervention reference: This element describes a study intervention (e.g., medication, treatment, therapy) for the clinical study. The human-readable description is provided in the Description element. The Coding element can be used to provide a machine-readable code for the indication (e.g., ATC M01AE01 code for \"Ibuprofen\" when used as a nonsteroidal anti-inflammatory drug)." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:studyIntervention ] ;
    sh:targetClass odm:StudyInterventions .

odm:StudyObjective a sh:NodeShape ;
    sh:closed true ;
    sh:description "The reason for performing a study in terms of the scientific questions to be answered by the analysis of data collected during the study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Defined level for the Study Objective" ;
            sh:in ( "Primary" "Secondary" "Exploratory" ) ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:level ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:description ],
        [ sh:description "Name of the study objective" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Unique identifier for the StudyObjective element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:StudyEndPointRef ;
            sh:description "StudyEndPointRef reference: A reference to a StudyEndPoint as it occurs within a specific StudyObjective." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:studyEndPointRef ] ;
    sh:targetClass odm:StudyObjective .

odm:StudyObjectives a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyObjectives is a container element for individual StudyObjective elements." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:StudyObjective ;
            sh:description "StudyObjective reference: The reason for performing a study in terms of the scientific questions to be answered by the analysis of data collected during the study." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:studyObjective ] ;
    sh:targetClass odm:StudyObjectives .

odm:StudyParameter a sh:NodeShape ;
    sh:closed true ;
    sh:description "A StudyParameter defines a study design parameter for which the value or values are delivered in the ParameterValue child element or elements." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:ParameterValue ;
            sh:description "ParameterValue reference: This element contains the value of the study parameter as text content." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:parameterValue ],
        [ sh:description "Unique identifier for StudyParameter. Note a StudyParameter can be referenced from other elements." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Short name or code for the parameter." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:shortName ],
        [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:coding ],
        [ sh:description "Longer name. Provides the full name of the parameter." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:term ] ;
    sh:targetClass odm:StudyParameter .

odm:StudyStructure a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyStructure element describes the general structure of a clinical study with arms, epochs, and workflows." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Arm ;
            sh:description "Arm reference: An Arm element provides the declaration of a study arm. Arms do not have any ordering relative to one another." ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path odm:arm ],
        [ sh:class odm:WorkflowRef ;
            sh:description "WorkflowRef reference: The WorkflowRef references a workflow definition" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:workflowRef ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:description ],
        [ sh:class odm:Epoch ;
            sh:description "Epoch reference: The planned period of subjects' participation in the trial is divided into sequential epochs. Each epoch is a period of time that serves a purpose in the trial as a whole. Epochs cannot overlap. The sequence of the epoch in the study is provided by the SequenceNumber attribute, the first epoch in the study being assigned the sequence number 1. Sequence numbers are subsequent." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path odm:epoch ] ;
    sh:targetClass odm:StudyStructure .

odm:StudySummary a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyParameter element allows to provide a set of study design parameters such as anticipated number of subjects, minimum and maximum age of the participants, or planned number of arms." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:StudyParameter ;
            sh:description "StudyParameter reference: A StudyParameter defines a study design parameter for which the value or values are delivered in the ParameterValue child element or elements." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:studyParameter ] ;
    sh:targetClass odm:StudySummary .

odm:StudyTargetPopulation a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyTargetPopulation describes the population targeted for the clinical study." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Coding ;
            sh:description "Coding reference: Coding references a symbol from a defined code system. It uses a code defined in a terminology system to associate semantics with a given term, codelist, variable, or group of variables. The presence of a Coding element associates a meaning to its parent element. Including multiple Coding elements for a given parent indicates synonymous meanings provided by different code systems or code system versions." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:coding ],
        [ sh:description "Human readable name for the StudyTargetPopulation." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:FormalExpression ;
            sh:description "FormalExpression reference: A FormalExpression used within a ConditionDef or a RangeCheck must evaluate to True or False. A FormalExpression referenced within a MethodDef having Type Imputation, Computation, or Transpose must evaluate to the correct DataType for an Item that may be imputed or computed using the Method. A FormalExpression gets parameter and return value definitions from the MethodSignature element. The data types in the MethodSignature parameters and return values must match the corresponding data types in the FormalExpression." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:formalExpression ],
        [ sh:description "Unique identifier for the StudyTargetPopulation element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:description ] ;
    sh:targetClass odm:StudyTargetPopulation .

odm:StudyTargetPopulationRef a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyTargetPopulationRef references a StudyTargetPopulation to which the estimand applies." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:studyTargetPopulationOID ] ;
    sh:targetClass odm:StudyTargetPopulationRef .

odm:StudyTiming a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyTiming element defines a timing constraint within the study, which can be an absolute timing constraint (e.g., start of the screening visit must be between 1 January 2022 and 31 December 2022), a relative timing constraint (e.g., visit 2 must be within 30 days after visit 1 with a window of +/- 1 week), a transition timing constraint (i.e., timing constraint on a transition within a defined workflow), or a duration timing constraint (e.g., the duration of visit 2 is planned to take hours with a window of 30 minutes)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Unique identifier for a StudyTiming element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Human readable identifier for a StudyTiming element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:class odm:DurationTimingConstraint ;
            sh:description "DurationTimingConstraint reference: The DurationTimingConstraint constrains the duration of an activity represented by a study, epoch, StudyEventGroupDef, StudyEventDef, ItemGroupDef, or ItemDef. It is used to constrain the duration of the visit, activity, or any other structural element." ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path odm:durationTimingConstraint ],
        [ sh:class odm:AbsoluteTimingConstraint ;
            sh:description "AbsoluteTimingConstraint reference: The element AbsoluteTimingConstraint is used to specify when an activity, represented by either a StudyEventGroup or StudyEvent, can take place." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path odm:absoluteTimingConstraint ],
        [ sh:class odm:RelativeTimingConstraint ;
            sh:description "RelativeTimingConstraint reference: The RelativeTimingConstraint element describes a relative timing constraint between 2 activities or groups of activities, represented by StudyEventGroups, StudyEvents, ItemGroups, or Items." ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path odm:relativeTimingConstraint ],
        [ sh:class odm:TransitionTimingConstraint ;
            sh:description "TransitionTimingConstraint reference: The TransitionTimingConstraint element defines a timing constraint on a transition between structural elements as defined in a workflow. As such, it is a non-blocking constraint. This means that the transition is set on hold as long as the timing condition is not fulfilled, and is executed as soon as the timing condition is fulfilled." ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path odm:transitionTimingConstraint ] ;
    sh:targetClass odm:StudyTiming .

odm:StudyTimings a sh:NodeShape ;
    sh:closed true ;
    sh:description "The StudyTimings element is a container element for individual StudyTiming elements." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:StudyTiming ;
            sh:description "StudyTiming reference: The StudyTiming element defines a timing constraint within the study, which can be an absolute timing constraint (e.g., start of the screening visit must be between 1 January 2022 and 31 December 2022), a relative timing constraint (e.g., visit 2 must be within 30 days after visit 1 with a window of +/- 1 week), a transition timing constraint (i.e., timing constraint on a transition within a defined workflow), or a duration timing constraint (e.g., the duration of visit 2 is planned to take hours with a window of 30 minutes)." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path odm:studyTiming ] ;
    sh:targetClass odm:StudyTimings .

odm:SubClass a sh:NodeShape ;
    sh:closed true ;
    sh:description "This element contains SubClass definitions." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Name of the SubClass" ;
            sh:in ( <nci:ExtCodeID:C176265> <nci:ExtCodeID:C177920> <nci:ExtCodeID:C172452> <nci:ExtCodeID:C165637> ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:name ],
        [ sh:description "For a nested or multilevel SubClass, the ParentClass attribute is used to define the hierarchy." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:parentClass ] ;
    sh:targetClass odm:SubClass .

odm:SubjectData a sh:NodeShape ;
    sh:closed true ;
    sh:description "Clinical data for a single subject." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:InvestigatorRef ;
            sh:description "InvestigatorRef reference: Provides a reference to the user who created the SubjectData record in the source system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path odm:investigatorRef ],
        [ sh:class odm:Annotation ;
            sh:description "Annotation reference: A general note about clinical data. If an annotation has both a comment and flags, the flags should be related to the comment." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 8 ;
            sh:path odm:annotation ],
        [ sh:class odm:Signature ;
            sh:description "Signature reference: An electronic signature applies to a collection of clinical data. This indicates that some user accepts legal responsibility for that data. See 21 CFR Part 11. The signature identifies the person signing, the location of signing, the signature meaning (via the referenced SignatureDef), the date and time of signing, and (in the case of a digital signature) an encrypted hash of the included data." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 7 ;
            sh:path odm:signature ],
        [ sh:class odm:SiteRef ;
            sh:description "SiteRef reference: Provides a reference to the site that the SubjectData record is associated with in the source system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path odm:siteRef ],
        [ sh:description "Identifies the transaction type when /ODM/@FileType is Transactional and there is no child element." ;
            sh:in ( "Insert" "Update" "Remove" "Upsert" "Context" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:transactionType ],
        [ sh:description "Unique identifier for the Subject." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:subjectKey ],
        [ sh:class odm:Query ;
            sh:description "Query reference: The Query element represents a request for clarification on a data item collected for a clinical trial, specifically a request from a sponsor or sponsor’s representative to an investigator to resolve an error or inconsistency discovered during data review. Queries can be created manually by individuals such as site monitors or data managers or automatically by systems. The full text of the Query exists in the Value child element. The optional Name attribute provide the means to provide a short identifier that can be included in listing or user interfaces." ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path odm:query ],
        [ sh:class odm:StudyEventData ;
            sh:description "StudyEventData reference: Clinical data for a study event (visit). The model supports repeating study events (e.g., when the same set of information is collected for a series of patient visits)." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:studyEventData ],
        [ sh:class odm:AuditRecord ;
            sh:description "AuditRecord reference: An AuditRecord carries information pertaining to the creation, deletion, or modification of clinical data. This information includes who performed that action, and where, when, and why that action was performed.AuditRecord information describes a change to clinical data, but is not itself clinical data. The value of some clinical data can always be changed by a subsequent transaction, but history cannot be changed, only added to." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:auditRecord ] ;
    sh:targetClass odm:SubjectData .

odm:Suffix a sh:NodeShape ;
    sh:closed true ;
    sh:description "This element may include credentials, or suffixes (e.g., Jr., III)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:Suffix .

odm:SummaryMeasure a sh:NodeShape ;
    sh:closed true ;
    sh:description "The SummaryMeasure element describes a summary measure for an estimand (e.g., proportion of patients with an improvement)." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:description ] ;
    sh:targetClass odm:SummaryMeasure .

odm:SupplementalDoc a sh:NodeShape ;
    sh:closed true ;
    sh:description "Supplemental data definitions" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class odm:DocumentRef ;
            sh:description "Links to a leaf element with the location of the document." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path odm:documentRef ] ;
    sh:targetClass odm:SupplementalDoc .

odm:TargetTransition a sh:NodeShape ;
    sh:closed true ;
    sh:description "TargetTransition provides a reference to a Transition element that is the target of a branching." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Reference to the Transition that is one of the targets of the branching." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:targetTransitionOID ],
        [ sh:description "Reference to a ConditionDef defining the condition under which the transition must be executed. The ConditionOID references a ConditionDef element defining a condition that needs to be evaluated at the time of entering the branching state. When the condition evaluates to true, the branch is entered." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:conditionOID ] ;
    sh:targetClass odm:TargetTransition .

odm:Title a sh:NodeShape ;
    sh:closed true ;
    sh:description "Text with the label for the document or dataset." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "multi-line text content from between XML tags" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path odm:content ] ;
    sh:targetClass odm:Title .

odm:Transition a sh:NodeShape ;
    sh:closed true ;
    sh:description "A Transition defines a link between 2 structural elements in a workflow. When the execution of the transition is dependent upon a timing constraint that is either directly defined or calculated, a TransitionTimingConstraint must be defined, referencing the current Transition." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "The StartConditionOID references a ConditionDef specifying a condition that must be met for the transition to start. For example, if the source structural element is a StudyEventGroupDef describing the activities for study screening and the target structural element is a StudyEventGroupDef describing study enrollment, the ConditionDef referenced by the StartConditionOID specifies the criteria that must be met for a subject to transition from screening to enrollment." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:startConditionOID ],
        [ sh:description "References the definition of the target structural element for the transition. The structural element may be a StudyEventGroupDef, StudyEventDef, ItemGroupDef, ItemDef, or Branching element. The latter will be used when there is a branching definition necessary as a result of the transition." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path odm:targetOID ],
        [ sh:description "Human readable name for the transition between two structural elements in a WorkflowDef element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "Unique identifier for the Transition element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "The EndConditionOID references a ConditionDef defining the condition under which the transition can be completed. As long as the condition is not met, the transition to the next actitivity or event must be considered to be temporary blocked. When the target structural element can be considered to be a \"visit,\" the condition can be regarded as a visit entry criterion." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path odm:endConditionOID ],
        [ sh:description "References the definition of the source structural element for the transition. The structural element may be a StudyEventGroupDef, StudyEventDef, ItemGroupDef, ItemDef, or Branching element." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:sourceOID ] ;
    sh:targetClass odm:Transition .

odm:TransitionTimingConstraint a sh:NodeShape ;
    sh:closed true ;
    sh:description "The TransitionTimingConstraint element defines a timing constraint on a transition between structural elements as defined in a workflow. As such, it is a non-blocking constraint. This means that the transition is set on hold as long as the timing condition is not fulfilled, and is executed as soon as the timing condition is fulfilled." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Unique identifier." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:description "Defines how the timing is to be defined between the two activities, starting from the start or the end of the source activity, and ending at the start or the end of the target activity." ;
            sh:in ( "StartToStart" "StartToFinish" "FinishToStart" "FinishToFinish" ) ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path odm:type ],
        [ sh:description "Specifies the amount of time after the TimepointTarget, the time between the two activities, may be lengthened." ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path odm:timepointPostWindow ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:description ],
        [ sh:description "Specifies the amount of time prior to the TimepointTarget, the time between the two activities, may be shortened." ;
            sh:maxCount 1 ;
            sh:order 6 ;
            sh:path odm:timepointPreWindow ],
        [ sh:description "R eferences a MethodDef that returns a durationDatetime. Use of a method allows timings that depend on other pieces of information, or complex timing calculations." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:methodOID ],
        [ sh:description "Human-readable name." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path odm:name ],
        [ sh:description "The planned time between the 2 activities defined by the transition in the workflow." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path odm:timepointTarget ],
        [ sh:description "References the workflow Transition on which the timing constraint must be executed." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path odm:transitionOID ] ;
    sh:targetClass odm:TransitionTimingConstraint .

odm:TrialPhase a sh:NodeShape ;
    sh:closed true ;
    sh:description "The TrialPhase element designates the phase of the study in the clinical trial." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Human-readable designation of the trial phase." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:value ],
        [ sh:class odm:Description ;
            sh:description "Description reference: A free-text description of the containing metadata component, unless restricted by Business Rules." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 1 ;
            sh:path odm:description ] ;
    sh:targetClass odm:TrialPhase .

odm:User a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about a specific user of a clinical data collection or data management system." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Business Rules" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path odm:oID ],
        [ sh:class odm:UserName ;
            sh:description "UserName reference: The user's login identification in the sender's system." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 4 ;
            sh:path odm:userName ],
        [ sh:class odm:GivenName ;
            sh:description "GivenName reference: The user's initial given name or all given names." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 8 ;
            sh:path odm:givenName ],
        [ sh:class odm:Telecom ;
            sh:description "Telecom reference: The telecommunication contacts points of a user, a location, or an organization. The Type attribute designates the type of contact." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 12 ;
            sh:path odm:telecom ],
        [ sh:class odm:FamilyName ;
            sh:description "FamilyName reference: The user's surname (family name)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 9 ;
            sh:path odm:familyName ],
        [ sh:class odm:FullName ;
            sh:description "FullName reference: The user's full formal name. May be a combination of Prefix, GivenName, FamilyName & Suffix. Intended to be used for display." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 7 ;
            sh:path odm:fullName ],
        [ sh:class odm:Suffix ;
            sh:description "Suffix reference: This element may include credentials, or suffixes (e.g., Jr., III)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path odm:suffix ],
        [ sh:class odm:Image ;
            sh:description "Image reference: A visual depiction of the user." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 10 ;
            sh:path odm:image ],
        [ sh:description "User's role in the study." ;
            sh:in ( "Sponsor" "Investigator" "Lab" "Other" "Subject" "Monitor" "Data analyst" "Care provider" "Assessor" ) ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path odm:userType ],
        [ sh:description "Reference to an Organization elment." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path odm:organizationOID ],
        [ sh:description "Reference to a Location element." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path odm:locationOID ],
        [ sh:class odm:Prefix ;
            sh:description "Prefix reference: Title or other prefix. Maps to FHIR HumanName.prefix (https://www.hl7.org/fhir/datatypes.html#humanname)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 5 ;
            sh:path odm:prefix ],
        [ sh:class odm:Address ;
            sh:description "Address reference: The postal address for a user, location, or organization." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 11 ;
            sh:path odm:address ] ;
    sh:targetClass odm:User .

odm:UserName a sh:NodeShape ;
